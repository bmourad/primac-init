SUBROUTINE TESTMETHOD
$INCLUDE ICS.CPYLIB INVENTORY
$INCLUDE ICS.CPYLIB CATEGORY
$INCLUDE WWINSERT RBO.H
OPEN "","INVENTORY" TO INVENTORY ELSE STOP
OPEN "","CATEGORY" TO CATEGORY ELSE STOP
  SITEMS = ""
  RITEMS = ""
  GITEMS = ""
  CMD = "SSELECT INVENTORY WITH CONO = '001'"
  UDTEXECUTE CMD	
	DATA = 1	
	LOOP
  	READNEXT ID THEN
		  MATREAD INV.REC FROM INVENTORY, ID THEN
        MATREAD CATG.REC FROM CATEGORY,'001':INV.LINE THEN
          IF CATG.TRK.LVL = 'S' THEN SITEMS<1,-1> = ID
          IF CATG.TRK.LVL = 'G' THEN GITEMS<1,-1> = ID
          IF CATG.TRK.LVL = 'R' THEN RITEMS<1,-1> = ID
        END
      END
    END ELSE
		  DATA = 0
  	END
  WHILE DATA DO
  REPEAT
  ALLVALS = ""
  ALLVALS = GITEMS : "еее" : RITEMS : "еее" : SITEMS
  STATUS = RBO.setProperty("","ServerMessage",ALLVALS)
RETURN

