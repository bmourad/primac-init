SUBROUTINE KEYEXM_UPDATE
$INCLUDE WWINSERT RBO.H

STATUS=RBO.getProperty('','ID',ID)

CONO=ID[1,3]
ERRMSG = ""
NAME = ""

STATUS=RBO.getProperty('','Keywords',NAME)

OPEN '','PREFIX' TO PREFIX ELSE ERRMSG = 'PREFIX FILE IS MISSING'; GOSUB 91000
NAMES<1>=NAME
WRITE NAMES ON PREFIX, CONO
RETURN


91000 
STATUS=RBO.setProperty('','ServerStatus',1)
STATUS=RBO.setProperty('','ServerMessage',ERRMSG)
RETURN

