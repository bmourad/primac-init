SUBROUTINE VENDI_VPDS_REC_UN_COST_CONVERSION
********************************************************************************
*   Program name :- VENDI_VPDS_REC_UN_COST_CONVERSION
*   Created:- 11/11/2005
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H

* Insert method code here
VPDS_ORD_UN_COST = ''
VPDS_REC_UN_COST= ''
STATUS = RBO.getProperty('','VPDS_ORD_UN_COST',VPDS_ORD_UN_COST)
STATUS = RBO.getProperty('','VPDS_REC_UN_COST',VPDS_REC_UN_COST)

NCNT = DCOUNT(VPDS_REC_UN_COST, @VM)
RCNT = DCOUNT(VPDS_ORD_UN_COST, @VM)

FOR I = 1 TO NCNT
	VPDS_REC_UN_COST<1,I> = ICONV(VPDS_REC_UN_COST<1,I>,"MD2,")
	VPDS_REC_UN_COST<1,I> = OCONV(VPDS_REC_UN_COST<1,I>,"MD4")
NEXT

FOR I = 1 TO RCNT	
	VPDS_ORD_UN_COST<1,I> = ICONV(VPDS_ORD_UN_COST<1,I>,"MD2,")
	VPDS_ORD_UN_COST<1,I> = OCONV(VPDS_ORD_UN_COST<1,I>,"MD4")
NEXT



STATUS = RBO.setProperty('','VPDS_REC_UN_COST',VPDS_REC_UN_COST)

STATUS = RBO.setProperty('','VPDS_ORD_UN_COST',VPDS_ORD_UN_COST)


* End of method code
RETURN

