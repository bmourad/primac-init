SUBROUTINE EstFormulaQ_READ
********************************************************************************
*   Program name :- EstFormulaQ_READ
*   Created:- 10/14/2003
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
 $INCLUDE WWINSERT RBO.H
  $INCLUDE CPYLIB CHAR
*
*---- OPEN FILES
*
  IF FILEINFO(CTLGTB,0) = 0 THEN
    OPEN 'CTLGTB' TO CTLGTB ELSE
      ERRMSG = 'CANNOT OPEN CATALOG TABLE FILE'
	SVR = "1"
      GOTO 93000
    END
  END
*
*---- MAIN PROCESS
*
  STATUS = RBO.getProperty('','ID',ID)
  STATUS = RBO.getProperty('','FPARAM',FPARAM)
  CONO = ID[1,3]
  READ DUMMY FROM CTLGTB, "EST.FORMULA.Q":FPARAM THEN
	SVR = "0"
	ERRMSG = "VALID FORMULA"
	GOTO 93000	
  END ELSE
	SVR = "1"
       ERRMSG = "INVALID FORMULA ID - EST.FORMULA.Q":FPARAM
       GOTO 93000
  END
RETURN

93000 *
	STATUS = RBO.setProperty('','ServerStatus',SVR)
	STATUS = RBO.setProperty('','ServerMessage',ERRMSG)
  RETURN
END

