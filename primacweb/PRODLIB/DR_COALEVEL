SUBROUTINE DR_COALEVEL(TVO.DIV,TVO.DEPT,TVO.CCTR,COA.LEVEL,DISABLEDCNT)
*TVO.DIV,TVO.DEPT,TVO.CCTR,COA.LEVEL,XYZ,ERRM
********************************************************************************
*   Program name :- DR_COALEVEL
*   Created:- 5/25/2004
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
* Insert method code here
GEN.DIV  = "00"
GEN.DEPT = "00"
GEN.CCTR = "000"
IF COA.LEVEL = 0 THEN 
	TVO.DIV      = GEN.DIV
	TVO.DEPT     = GEN.DEPT 
	TVO.CCTR     = GEN.CCTR
	DISABLEDCNT  = "DDV#DDP#DCtr"
END ELSE
	DIV = TVO.DIV 
	IF COA.LEVEL = 1 OR DIV = GEN.DIV THEN
		TVO.DEPT     = GEN.DEPT
		TVO.CCTR     = GEN.CCTR
		DISABLEDCNT  = "DDV#DDP#DCtr"
	END ELSE
		DEPT = TVO.DEPT 
		IF COA.LEVEL = 2 OR DEPT = GEN.DEPT THEN
			TVO.CCTR     = GEN.CCTR
			DISABLEDCNT  = "DDV#DDP#DCtr"	
		END ELSE
			CCTR 	      = TVO.CCTR 
			DISABLEDCNT  = "DDV#DDP#DCtr"
		END
	END				
END


* End of method code
RETURN

