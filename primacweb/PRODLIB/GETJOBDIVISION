SUBROUTINE GETJOBDIVISION
********************************************************************************
*   Program name :- GETJOBDIVISION
*   Created:- 10/5/2006
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE JCS.CPYLIB JOB

OPEN '','JOB' TO JOB ELSE
   ERRMSG = 'JOB FILE IS MISSING'; GOTO 93000
END


STATUS=RBO.getProperty('','OPO_JOB_NO',JOBNO)
MATREAD JOB.REC FROM JOB,JOBNO THEN
   STATUS=RBO.setProperty('','JobDivision',JOB.DIV)
END
RETURN

93000* 
   
  STATUS = RBO.setProperty('','ServerStatus',1)        
  STATUS = RBO.setProperty('','ServerMessage',ERRMSG) 
RETURN
