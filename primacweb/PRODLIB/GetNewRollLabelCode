SUBROUTINE GetNewRollLabelCode
********************************************************************************
*   Program name :- GetNewRollLabelCode
*   Created:- 6/27/2003
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
********************************************************************************
$INCLUDE WWINSERT RBO.H

* Insert method code here
   ERRMSG = ''
   CantGetProperty='Cannot get property '
   CantSetProperty='Cannot set property '

* Open files
   OPEN '','CONTROL' TO CONTROL ELSE
      ERRMSG='Cannot open CONTROL file!'
      GOTO 93000
   END

* Get all properties
   STATUS = RBO.getProperty('','PMCProperty',PMCProperty)

* Get CONO
   CONO=PMCProperty<1,4>

NUM =0

READU NUM FROM CONTROL,CONO:"EST.SEQ.NUM" ELSE NUM=10001
      SEQ="01"
      EST.KEY=NUM :"-":SEQ
      NUM=NUM+1
      WRITE NUM ON CONTROL,CONO:"EST.SEQ.NUM"

*
* Set properties

   STATUS = RBO.setProperty('','ID',CONO:EST.KEY)
   IF STATUS THEN ERRMSG=CantSetProperty:'CONO:EST.KEY'; GOTO 93000
   GOTO 99999
93000 
   IF ERRMSG THEN
      CALL RBO_ERROR_SUB(ERRMSG)
   END
* End of method code
99999 
  RETURN

