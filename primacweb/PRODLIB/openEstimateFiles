SUBROUTINE openEstimateFiles(ERRMSG,IN_PARAM,OUT_PARAM,MAT FILE.VARS,MAT JES.FILE.VARS)

********************************************************************************
*   Program name :- openEstimateFiles
*   Created:- 6/2/2003
*   Author :- Irfan M Saleem
*  This program opens all of the files needed for entering an estimate
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE JES.CPYLIB JES.FILE.VARS
$INCLUDE CPYLIB FILE.VARS
$INCLUDE CPYLIB CHAR
*
*---- OPEN ALL FILES
*
   ERRMSG = ""
   IF FILEINFO(COMPANY,0) = 0 THEN
      OPEN '','COMPANY' TO COMPANY ELSE
         ERRMSG = 'Cannot open COMPANY file!'
         RETURN
      END
   END
   IF FILEINFO(CONTROL,0) = 0 THEN
      OPEN '','CONTROL' TO CONTROL ELSE
         ERRMSG = 'Cannot open CONTROL file!'
         RETURN
      END
   END
   IF FILEINFO(PREFIX,0) = 0 THEN
      OPEN '','PREFIX' TO PREFIX ELSE
         ERRMSG = 'Cannot open PREFIX file!'
         RETURN
      END
   END
   IF FILEINFO(CUSTOMER,0) = 0 THEN
      OPEN '','CUSTOMER' TO CUSTOMER ELSE
         ERRMSG = 'Cannot open CUSTOMER file!'
         RETURN
      END
   END
   IF FILEINFO(CUSTOMER.XREF,0) = 0 THEN
      OPEN '','CUSTOMER.XREF' TO CUSTOMER.XREF ELSE
         ERRMSG = 'Cannot open CUSTOMER.XREF file!'
         RETURN
      END
   END
   IF FILEINFO(SALESMAN,0) = 0 THEN
      OPEN '','SALESMAN' TO SALESMAN ELSE
         ERRMSG = 'Cannot open SALESMAN file!'
         RETURN
      END
   END
   IF FILEINFO(DEPARTMENT,0) = 0 THEN
      OPEN '','DEPARTMENT' TO DEPARTMENT ELSE
         ERRMSG = 'Cannot open DEPARTMENT file!'
         RETURN
      END
   END
   IF FILEINFO(DIVISION,0) = 0 THEN
      OPEN '','DIVISION' TO DIVISION ELSE
         ERRMSG = 'Cannot open DIVISION file!'
         RETURN
      END
   END
   IF FILEINFO(COST.CNTR,0) = 0 THEN
      OPEN '','COST.CNTR' TO COST.CNTR ELSE
         ERRMSG = 'Cannot open COST.CNTR file!'
         RETURN
      END
   END
   IF FILEINFO(OPERATION,0) = 0 THEN
      OPEN '','OPERATION' TO OPERATION ELSE
         ERRMSG = 'Cannot open OPERATION file!'
         RETURN
      END
   END
   IF FILEINFO(JOB,0) = 0 THEN
      OPEN '','JOB' TO JOB ELSE
         ERRMSG = 'Cannot open JOB file!'
         RETURN
      END
   END
   IF FILEINFO(JOB.CATEGORY,0) = 0 THEN
      OPEN '','JOB.CATEGORY' TO JOB.CATEGORY ELSE
         ERRMSG = 'Cannot open JOB.CATEGORY file!'
         RETURN
      END
   END
   IF FILEINFO(SALES.CODE,0) = 0 THEN
      OPEN '','SALES.CODE' TO SALES.CODE ELSE
         ERRMSG = 'Cannot open SALES.CODE file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATOR,0) = 0 THEN
      OPEN '','ESTIMATOR' TO ESTIMATOR ELSE
         ERRMSG = 'Cannot open ESTIMATOR file!'
         RETURN
      END
   END
   IF FILEINFO(VEND,0) = 0 THEN
      OPEN '','VEND' TO VEND ELSE
         ERRMSG = 'Cannot open VEND file!'
         RETURN
      END
   END
   IF FILEINFO(CATEGORY,0) = 0 THEN
      OPEN '','CATEGORY' TO CATEGORY ELSE
         ERRMSG = 'Cannot open CATEGORY file!'
         RETURN
      END
   END
   IF FILEINFO(CATEGORY.OSP,0) = 0 THEN
      OPEN '','CATEGORY.OSP' TO CATEGORY.OSP ELSE
         ERRMSG = 'Cannot open CATEGORY.OSP file!'
         RETURN
      END
   END
   IF FILEINFO(SHIP.VIA,0) = 0 THEN
      OPEN '','SHIP.VIA' TO SHIP.VIA ELSE
         ERRMSG = 'Cannot open SHIP.VIA file!'
         RETURN
      END
   END
   IF FILEINFO(FOH.TABLE,0) = 0 THEN
      OPEN '','FOH.TABLE' TO FOH.TABLE ELSE
         ERRMSG = 'Cannot open FOH.TABLE file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.MARKUP,0) = 0 THEN
      OPEN '','ESTIMATE.MARKUP' TO ESTIMATE.MARKUP ELSE
         ERRMSG = 'Cannot open ESTIMATE.MARKUP file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE,0) = 0 THEN
      OPEN '','ESTIMATE' TO ESTIMATE ELSE
         ERRMSG = 'Cannot open ESTIMATE file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.DIV,0) = 0 THEN
      OPEN '','ESTIMATE.DIV' TO ESTIMATE.DIV ELSE
         ERRMSG = 'Cannot open ESTIMATE.DIV file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.GRP,0) = 0 THEN
      OPEN '','ESTIMATE.GRP' TO ESTIMATE.GRP ELSE
         ERRMSG = 'Cannot open ESTIMATE.GRP file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.GRP.XREF,0) = 0 THEN
      OPEN '','ESTIMATE.GRP.XREF' TO ESTIMATE.GRP.XREF ELSE
         ERRMSG = 'Cannot open ESTIMATE.GRP.XREF file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.MATL,0) = 0 THEN
      OPEN '','ESTIMATE.MATL' TO ESTIMATE.MATL ELSE
         ERRMSG = 'Cannot open ESTIMATE.MATL file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.TBL,0) = 0 THEN
      OPEN '','ESTIMATE.TBL' TO ESTIMATE.TBL ELSE
         ERRMSG = 'Cannot open ESTIMATE.TBL file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.PAPER.GROUP,0) = 0 THEN
      OPEN '','ESTIMATE.PAPER.GROUP' TO ESTIMATE.PAPER.GROUP ELSE
         ERRMSG = 'Cannot open ESTIMATE.PAPER.GROUP file!'
         RETURN
      END
   END
   IF FILEINFO(EST.DESC.CODE,0) = 0 THEN
      OPEN '','EST.DESC.CODE' TO EST.DESC.CODE ELSE
         ERRMSG = 'Cannot open EST.DESC.CODE file!'
         RETURN
      END
   END
   IF FILEINFO(EQUIPMENT,0) = 0 THEN
      OPEN '','EQUIPMENT' TO EQUIPMENT ELSE
         ERRMSG = 'Cannot open EQUIPMENT file!'
         RETURN
      END
   END
   IF FILEINFO(COLOR,0) = 0 THEN
      OPEN '','COLOR' TO COLOR ELSE
         ERRMSG = 'Cannot open COLOR file!'
         RETURN
      END
   END
   IF FILEINFO(FINISH,0) = 0 THEN
      OPEN '','FINISH' TO FINISH ELSE
         ERRMSG = 'Cannot open FINISH file!'
         RETURN
      END
   END
   IF FILEINFO(INVENTORY,0) = 0 THEN
      OPEN '','INVENTORY' TO INVENTORY ELSE
         ERRMSG = 'Cannot open INVENTORY file!'
         RETURN
      END
   END
   IF FILEINFO(INV.WHSE,0) = 0 THEN
      OPEN '','INV.WHSE' TO INV.WHSE ELSE
         ERRMSG = 'Cannot open INV.WHSE file!'
         RETURN
      END
   END
   IF FILEINFO(PRODUCT.XREF,0) = 0 THEN
      OPEN '','PRODUCT.XREF' TO PRODUCT.XREF ELSE
         ERRMSG = 'Cannot open PRODUCT.XREF file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.XREF,0) = 0 THEN
      OPEN '','ESTIMATE.XREF' TO ESTIMATE.XREF ELSE
         ERRMSG = 'Cannot open ESTIMATE.XREF file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.BIND.SPOIL,0) = 0 THEN
      OPEN '','ESTIMATE.BIND.SPOIL' TO ESTIMATE.BIND.SPOIL ELSE
         ERRMSG = 'Cannot open ESTIMATE.BIND.SPOIL file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.PRESS.SPOIL,0) = 0 THEN
      OPEN '','ESTIMATE.PRESS.SPOIL' TO ESTIMATE.PRESS.SPOIL ELSE
         ERRMSG = 'Cannot open ESTIMATE.PRESS.SPOIL file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.RUN.EFF,0) = 0 THEN
      OPEN '','ESTIMATE.RUN.EFF' TO ESTIMATE.RUN.EFF ELSE
         ERRMSG = 'Cannot open ESTIMATE.RUN.EFF file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.CUT.EFF,0) = 0 THEN
      OPEN '','ESTIMATE.CUT.EFF' TO ESTIMATE.CUT.EFF ELSE
         ERRMSG = 'Cannot open ESTIMATE.CUT.EFF file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.DEPT,0) = 0 THEN
      OPEN '','ESTIMATE.DEPT' TO ESTIMATE.DEPT ELSE
         ERRMSG = 'Cannot open ESTIMATE.DEPT file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.WORK,0) = 0 THEN
      OPEN '','ESTIMATE.WORK' TO ESTIMATE.WORK ELSE
         ERRMSG = 'Cannot open ESTIMATE.WORK file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.RL,0) = 0 THEN
      OPEN '','ESTIMATE.RL' TO ESTIMATE.RL ELSE
         ERRMSG = 'Cannot open ESTIMATE.RL file!'
         RETURN
      END
   END
   IF FILEINFO(ESTIMATE.RPT,0) = 0 THEN
      OPEN '','ESTIMATE.RPT' TO ESTIMATE.RPT ELSE
         ERRMSG = 'Cannot open ESTIMATE.RPT file!'
         RETURN
      END
   END
   IF FILEINFO(CUST.EST.XREF,0) = 0 THEN
      OPEN '','CUST.EST.XREF' TO CUST.EST.XREF ELSE
         ERRMSG = 'Cannot open CUST.EST.XREF file!'
         RETURN
      END
   END
   IF FILEINFO(PROSPECT.EST.XREF,0) = 0 THEN
      OPEN '','PROSPECT.EST.XREF' TO PROSPECT.EST.XREF ELSE
         ERRMSG = 'Cannot open PROSPECT.EST.XREF file!'
         RETURN
      END
   END
   IF FILEINFO(XREF.DATA,0) = 0 THEN
      OPEN '','XREF.DATA' TO XREF.DATA ELSE
         ERRMSG = 'Cannot open XREF.DATA file!'
         RETURN
      END
   END
   IF FILEINFO(PMC_REPORTS,0) = 0 THEN
      OPEN '','PMC_REPORTS' TO PMC_REPORTS ELSE
         ERRMSG = 'Cannot open PMC_REPORTS file!'
         RETURN
      END
   END
   RETURN



* End of method code
RETURN

