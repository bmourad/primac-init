SUBROUTINE DELIVERY_TICKET_ALIGN
********************************************************************************
*   Program name :- DELIVERY_TICKET_ALIGN
*   Created:- 2/18/2005
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H

  OPEN '','_HOLD_' TO HOLD.FILE ELSE ERRMSG = 'CANNOT LOCATE THE _HOLD_ FILE!' ; STOP
* Insert method code here
*
*---- INITIALIZATION
*
  STMT = 'SETPTR ,,,,,3,BRIEF,DEST \\Creative_domain\hpLaserJ,BANNER DEL_TIC_ALIGN'
*  STMT = 'SETPTR ,,,,,3,BRIEF,BANNER RK1'
  UDTEXECUTE STMT CAPTURING RESULT

      SP=" ";SP2="  ";SP3="   "
      SP12=STR(" ",12);SP51=STR(" ",51);SP74=STR(" ",74)
      ADDR.ALIGN=STR("X",35);CITY.ST.ALIGN="XXXXXXXXXX, XX  99999"
      ATTEN="ATTN: ":STR("X",26)
      QTY.ALIGN="999,999,999";DESC.ALIGN=STR("X",70);PRICE.ALIGN=" 999,999.99"
      DATE="##/##/##";NUM=STR("9",15);SHIP.VIA=STR("X",25)
      ERRMSG = ''
      PRINTER ON

*---- MAIN PROCESSING
*
10*
*T21147 v
*     FOR P=1 TO 6
      FOR P=1 TO 9
         PRINT
      NEXT P
      PRINT SP74:DATE
      FOR P=1 TO 6
         PRINT
      NEXT P
      FOR AA=1 TO 4
         PRINT SP12:ADDR.ALIGN 
      NEXT AA
      PRINT SP12:CITY.ST.ALIGN 
      PRINT SP12:ATTEN
      PRINT;PRINT;PRINT
      PRINT SP3:NUM[1,8]:SP2:NUM[1,10]:SP:SHIP.VIA:SP:NUM
      PRINT;PRINT
      FOR AA=1 TO 12
         PRINT SP:QTY.ALIGN:SP2:DESC.ALIGN
      NEXT AA
      PRINT;PRINT
      PRINT SP51:PRICE.ALIGN[2,10]:SP:PRICE.ALIGN[2,10]:SP:PRICE.ALIGN
      PRINT;PRINT
	CMD = 'SPOOL _HOLD_ DEL_TIC_ALIGN -O'
       UDTEXECUTE CMD CAPTURING JUNK
	DELETE HOLD.FILE,"DEL_TIC_ALIGN"

      PRINTER OFF
      PRINTER CLOSE       
      RETURN
      END

* End of method code

