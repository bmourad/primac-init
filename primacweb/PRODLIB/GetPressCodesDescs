SUBROUTINE GetPressCodesDescs
********************************************************************************
*   Program name :- GetPressCodesDescs
*   Created:- 5/20/2003
*   Created By :- Jaweed,S.A
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE JES.CPYLIB EQUIPMENT

ERRMSG1 = RBO.getProperty('','ID',ID)

* Insert method code here
  OPEN "","COST.CNTR" TO COST.CNTR ELSE
    ERRMSG = "CANNOT OPEN COST.CNTR FILE"
  END

  OPEN "","EQUIPMENT" TO EQUIPMENT ELSE
    ERRMSG = "CANNOT OPEN EQUIPMENT FILE"
  END
  

Press.Codes.List = ''
Press.Descs.List = ''
EQUIP.TYPE.LIST = ''
 
 CONO = ID[1,3]

  SELECT COST.CNTR
LOOP
	EOF = 1
	READNEXT EQUIP.ID ELSE EOF = 0
WHILE EOF DO

		MATREAD EQUIPMENT.REC FROM EQUIPMENT,EQUIP.ID THEN 
			IF EQP.TYPE = 01 THEN
				EQUIP.TYPE.LIST<1,-1> = EQP.TYPE
	
				READV CCTR.DESC FROM COST.CNTR, EQUIP.ID<0>, 1 THEN
					Press.Codes.List<1,-1> = EQUIP.ID<0>[4,99]
					Press.Descs.List<1,-1> = CCTR.DESC
				END ELSE 
					Press.Codes.List<1,-1> = EQUIP.ID<0>[4,99]
					Press.Descs.List<1,-1> = EQUIP.ID<0>[4,99]
				END
	
			END ELSE
			END
		END ELSE
				READV CCTR.DESC FROM COST.CNTR, EQUIP.ID<0>, 1 THEN
					Press.Codes.List<1,-1> = EQUIP.ID<0>[4,99]
					Press.Descs.List<1,-1> = CCTR.DESC
				END ELSE 
					Press.Codes.List<1,-1> = EQUIP.ID<0>[4,99]
					Press.Descs.List<1,-1> = EQUIP.ID<0>[4,99]
				END

		END
REPEAT
SWAP "'" WITH "" IN Press.Descs.List
SWAP '"' WITH "" IN Press.Descs.List
SWAP "." WITH "" IN Press.Descs.List

	STATUS=RBO.setProperty('','PressCodes',Press.Codes.List)
	STATUS=RBO.setProperty('','PressCodesDescs',Press.Descs.List)
	
	STATUS=RBO.setProperty('','ServerStatus',1)
	STATUS=RBO.setProperty('','ServerMessage',ERRMSG )

* End of method code
RETURN
