SUBROUTINE Estimates
********************************************************************************
*   Program name :- Estimates
*   Created:- 8/24/2004
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE JES.CPYLIB ESTIMATE

 OPEN '','ESTIMATE' TO ESTIMATE ELSE ERRMSG='ESTIMATE FILE IS MISSING'; GOTO 93000

STATUS=RBO.getProperty('','PMCProperty',PMCProperty)


CONO   = PMCProperty<1,4>

Estimates = ''

CMD1 = " SSELECT ESTIMATE WITH CONO= '" : CONO : "'"

UDTEXECUTE CMD1
EOF = 1
LOOP
	READNEXT ID ELSE EOF = 0		
WHILE EOF DO
	Estimates<1,-1> =ID[4,99] 	
REPEAT

	STATUS=RBO.setProperty('','Estimates',Estimates)
93000
	STATUS=RBO.setProperty('','ServerStatus',1)
	STATUS=RBO.setProperty('','ServerMessage',ERRMSG )

RETURN

