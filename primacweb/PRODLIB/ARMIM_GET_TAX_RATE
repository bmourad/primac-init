SUBROUTINE ARMIM_GET_TAX_RATE
***************************************************************************
* REVISION    - [e12.0]
* Copyright 1982 by Computer Business Associates (Vercom Software, Inc.)
* SYSTEM    - PRIMAC
* PROGRAM   - ARMIM_GET_TAX_RATE
* AUTHOR    - ABED ALI
* DATE      - 5/17/2003
****************************************************************************
* In Properties:
* --------------
* Cono,ID(TAX_ID)
* Out Properties:
* ---------------
* TAX_RATE
****************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE PMC.CPYLIB TAX
* Insert method code here
TYPE = ''
ERRMSG = ''
OPEN "","TAX" TO TAX ELSE
       TYPE='F'
	ERRMSG="Unable to open TAX File"
	GOTO 9999
END
STATUS = RBO.getProperty('','Cono',CONO)
STATUS = RBO.getProperty('','TAX_ID',ID)

MATREAD TAX.REC FROM TAX, CONO:ID ELSE
	TYPE = '1'
       ERRMSG="Jurisdiction ":ID:" is not on file"
       GOTO 9999
END

STATUS = RBO.setProperty('','TAX_RATE',OCONV(TAX.RATE,"MD3"))
*STATUS = RBO.setProperty('','INC_CATEGORY',INC_CATEGORY)

9999*
IF ERRMSG # '' THEN
	STATUS = RBO.setProperty('','ServerStatus',TYPE)
	STATUS = RBO.setProperty('','ServerMessage',ERRMSG)
END
RETURN

* End of method code
RETURN

