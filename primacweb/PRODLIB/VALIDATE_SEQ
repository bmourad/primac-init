SUBROUTINE VALIDATE_SEQ
********************************************************************************
*   Program name :- VALIDATE_SEQ
*   Created:- 9/06/2005  
*   Programmer :- Suhail Hussain S
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE JCS.CPYLIB DAILY.REVERSE

  ERRMSG = ''

  OPEN '','DAILY.REVERSE' TO DAILY.REVERSE ELSE ERRMSG='DAILY.REVERSE FILE IS MISSING'; GOTO 93000

  STATUS=RBO.getProperty('','ID',SEQ_ID)
  
  MATREAD DRV.REC FROM DAILY.REVERSE,SEQ_ID THEN
     STATUS = RBO.setProperty("","VALID_SEQ",1)
  END ELSE
     STATUS = RBO.setProperty("","VALID_SEQ",0)
  END
RETURN

93000:
   STATUS = RBO.setProperty('','ServerStatus',1)
   STATUS = RBO.setProperty('','ServerMessage',ERRMSG)
* End of method code
RETURN

