SUBROUTINE EPRIMAC_LOCKS_RELEASERECORD
********************************************************************************
*   Program name :- EPRIMAC_LOCKS_RELEASERECORD
*   Created:- 8/10/2005
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE PMC.CPYLIB SHIP.VIA
ID = ''
RECORD_ID = ''

OPEN '','SHIP.VIA' TO SHIP.VIA ELSE ERRMSG = 'SHIP.VIA FILE MISSING';GOTO 93000
STATUS = RBO.getProperty('','ID',ID)
STATUS = RBO.getProperty('','RECORD_ID',RECORD_ID)

IF RECORDLOCKED(SHIP.VIA, RECORD_ID) # 0 THEN
	RELEASE SHIP.VIA, RECORD_ID
	*DELETE SHIP.VIA, ID
END
STATUS=RBO.callMethod('','DeleteData')
RETURN

93000:
	STATUS = RBO.setProperty('','ServerStatus',1)
	STATUS = RBO.setProperty('','ServerMessage',ERRMSG)
RETURN

