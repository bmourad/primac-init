SUBROUTINE ESTMPS_ReadData
********************************************************************************
*   Program name :- ESTMPS_ReadData
*   Created:- 4/13/2005
*------------------------------------------------------------------------------*
$INCLUDE WWINSERT RBO.H
$INCLUDE CPYLIB SCOMMON1
$INCLUDE JES.CPYLIB ESTIMATE.PURGE
$INCLUDE PMC.CPYLIB COMPANY 
$INCLUDE CPYLIB CHAR
$INCLUDE CPYLIB SYSCOM

  OPEN "","COMPANY" TO COMPANY ELSE
      ERRMSG = "CANNOT OPEN COMPANY FILE"
      GOSUB 90000
      STOP
  END
  OPEN "","CONTROL" TO CONTROL ELSE
      ERRMSG = "CANNOT OPEN CONTROL FILE"
      GOSUB 90000
      STOP
  END


  STATUS=RBO.getProperty('','Cono',CONO)
  MATREADU ESTP.REC FROM CONTROL, CONO:"MANUAL.PURGE" ELSE
      MAT ESTP.REC = ""
      NEW.REC = 1
  END

STATUS=RBO.setProperty('','PPDMVField01',ESTP.FROM.EST)
STATUS=RBO.setProperty('','PPDMVField02',ESTP.TO.EST)
STATUS=RBO.setProperty('','PPDMVField03',ESTP.DIV)

90000:
STATUS=RBO.setProperty('','ServerMessage',ERRMSG)
RETURN
