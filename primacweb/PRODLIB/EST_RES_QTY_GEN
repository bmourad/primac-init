			CALL EST_TABLE_FCTR (CONO, TEMP.QTY, TBL.ID, TEMP.GRP.FCTR)
                     IF TEMP.FCTR+0 = 0 THEN TEMP.FCTR = 1000
               END CASE
            CASE 1
