			VALUECONTENT = TEMP.GRP.QPARAM :"^": "R" :"^": EQTY :"^": DEPT :"^": COMP :"^": "PRE" :"^": "C" :"^": EST.KEY :"^": TEMP.TYPE :"^": TEMP.CCTR :"^": TEMP.OPV :"^":TPTR:"^": TEMP.QTY :"^": TEMP.GRP.QTY :"^": TEMP.CODE  :"^ "
