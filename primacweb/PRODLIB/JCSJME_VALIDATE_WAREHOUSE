SUBROUTINE JCSJME_VALIDATE_WAREHOUSE
********************************************************************************
*   Program name :- JCSJME_VALIDATE_WAREHOUSE
*   Created:- 10/7/2003
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE ICS.CPYLIB WAREHOUSE
* Insert method code here
OPEN "","WAREHOUSE" TO WAREHOUSE ELSE ERRMSG="FILE WAREHOUSE IS MISSING";GOTO 93000

STATUS=RBO.getProperty('','Cono',CONO)
STATUS=RBO.getProperty('','DMT_WHSE',DMT_WHSE)
STATUS=RBO.getProperty('','DMT_DIV',JOB.DIV)

WHSE.ID=CONO:DMT_WHSE

MATREAD WHSE.REC FROM WAREHOUSE,WHSE.ID ELSE MAT WHSE.REC=''
IF JOB.DIV # WHS.DIV AND WHS.DIV # "00" THEN
  ERRMSG="INVENTORY WAREHOUSE DIVISION DOES NOT MATCH JOB DIVISION"
  GOSUB 93000
END
STATUS = RBO.setProperty('','PMCProperty',WHS.DIV:"!!":JOB.DIV)
* End of method code
RETURN

93000*
STATUS = RBO.setProperty('','ServerStatus',1)
STATUS = RBO.setProperty('','ServerMessage',ERRMSG)

