SUBROUTINE JCSCME_JOB_SHIP
********************************************************************************
*   Program name :- JCSCME_JOB_SHIP
*   Created:- 9/17/2003
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE JCS.CPYLIB JOB.SHIP
* Insert method code here


  OPEN '','JOB.SHIP' TO JOB.SHIP ELSE ERRMSG='JOB.SHIP FILE IS MISSING'; GOTO 93000

STATUS=RBO.getProperty('','PMCProperty',PMCProperty)
CONO   = PMCProperty<1,4>

JOB_LIST = ""
JOB_JURS = ""

CMD = "SSELECT JOB.SHIP WITH CONO = '" : CONO : "'"
UDTEXECUTE CMD

DATA = 1
LOOP
	READNEXT ID ELSE DATA = 0
WHILE DATA DO
	JOB_LIST<1,-1> = ID[4,99]
REPEAT

	JOB_COUNT = DCOUNT(JOB_LIST,@VM)
		FOR X = 1 TO JOB_COUNT
			MATREAD JSP.REC FROM JOB.SHIP,CONO:JOB_LIST<1,X> THEN
				JOB_JURS<1,X> = JSP.JURS
			END

		NEXT X

STATUS = RBO.setProperty('','JOB_LIST',JOB_LIST)
STATUS = RBO.setProperty('','JOB_JURS',JOB_JURS)
* End of method code
RETURN

93000*
STATUS = RBO.setProperty('','ServerStatus',1)
STATUS = RBO.setProperty('','ServerMessage',ERRMSG)

