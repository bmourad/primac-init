SUBROUTINE ESTMPS_WriteData
********************************************************************************
*   Program name :- ESTMPS_WriteData
*   Created:- 4/13/2005
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE CPYLIB SCOMMON1
$INCLUDE JES.CPYLIB ESTIMATE.PURGE
$INCLUDE PMC.CPYLIB COMPANY 
$INCLUDE CPYLIB CHAR
$INCLUDE CPYLIB SYSCOM

  OPEN "","COMPANY" TO COMPANY ELSE
      ERRMSG = "CANNOT OPEN COMPANY FILE"
      GOSUB 90000
      STOP
  END
  OPEN "","CONTROL" TO CONTROL ELSE
      ERRMSG = "CANNOT OPEN CONTROL FILE"
      GOSUB 90000
      STOP
  END
STATUS=RBO.getProperty('','PPDMVField01',FROM_EST)
STATUS=RBO.getProperty('','PPDMVField02',TO_EST)
STATUS=RBO.getProperty('','PPDMVField03',DIVI)

STATUS=RBO.getProperty('','Cono',CONO)

	ESTP.FROM.EST = FROM_EST
	ESTP.TO.EST = TO_EST
	ESTP.DIV = DIVI

MATWRITE ESTP.REC ON CONTROL,CONO:"MANUAL.PURGE"

90000:
STATUS=RBO.setProperty('','ServerMessage',ERRMSG)
RETURN

