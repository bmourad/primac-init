SUBROUTINE GetOutsideREQCodes
********************************************************************************
*   Program name :- GetOutsideREQCodes
*   Created:- 6/9/2004
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE PMC.CPYLIB COMPANY 

* Insert method code here
   OPEN '','COMPANY' TO COMPANY ELSE
      ERRMSG = 'COMPANY FILE IS MISSING'
      GOTO 93000
   END

STATUS=RBO.getProperty('','PMCProperty',PMCProperty)
USER.ID = PMCProperty<1,3>
CONO = PMCProperty<1,4>

MATREAD COMP.REC FROM COMPANY,CONO ELSE
	MAT COMP.REC = ""	
END 
STATUS=RBO.setProperty('','CO_NAME',CO.NAME)
STATUS=RBO.setProperty('','CO_ADDRESS',CO.ADDRESS)
STATUS=RBO.setProperty('','CO_CITY',CO.CITY)
STATUS=RBO.setProperty('','CO_STATE',CO.STATE)
STATUS=RBO.setProperty('','CO_ZIP',CO.ZIP)

OUT.PO.LIST = ""
*CMD = "SSELECT OUTSIDE.PO WITH CONO LIKE '" : CONO : "...'"
CMD = "SSELECT OUT.REQ WITH CONO LIKE '" : CONO : "...'"


UDTEXECUTE CMD
DATA = 1
LOOP
*	READNEXT OUT.PO.ID THEN
*		OUT.PO.LIST<1,-1> = OUT.PO.ID[4,99]
	READNEXT OUT.REQ.ID THEN
		OUT.PO.LIST<1,-1> = OUT.REQ.ID[4,99]
	END ELSE
		DATA = 0  
	END

WHILE DATA DO
REPEAT

STATUS = RBO.setProperty('','OUT_PO_LIST',OUT.PO.LIST)

*STATUS = RBO.setProperty('','ID',CMD)

* End of method code
RETURN
93000*
STATUS=RBO.setProperty('','ServerMessage',ERRMSG)



