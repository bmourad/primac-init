SUBROUTINE VALIDATE_INV_SERIALS
********************************************************************************
*   Program name :- VALIDATE_INV_SERIALS
*   Created:- 7/20/2007
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE ICS.CPYLIB INV_SERIAL
* Insert method code here

OPEN "","INV_SERIAL" TO INV_SERIAL ELSE
    ERRMSG = "CANNOT OPEN INV_SERIAL FILE"
    GOTO 93000
END
ERRMSG = ''
ISTK_PO_NO = ''

STATUS = RBO.getProperty('','ID',ID)

      MATREAD ISTK.REC FROM INV_SERIAL,ID THEN
		ISTK_PO_NO =ISTK.PO.NO
      END ELSE
		ERRMSG = ID[4,99]:" item ID missing from INV_SERIAL"
		GOTO 93000
      END
    
STATUS = RBO.setProperty('','ISTK_PO_NO',ISTK_PO_NO)
  * End of method code
RETURN
93000 *
    STATUS = RBO.setProperty('','ServerStatus',1)
    STATUS = RBO.setProperty('','ServerMessage',ERRMSG)
RETURN
