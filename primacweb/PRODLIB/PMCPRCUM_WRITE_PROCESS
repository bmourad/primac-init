SUBROUTINE PMCPRCUM_WRITE_PROCESS

$INCLUDE WWINSERT RBO.H
$INCLUDE PMC.CPYLIB SECURITY
$INCLUDE CPYLIB CHAR
ERRMSG = ''
CONO = ''
USER.ID = ''
PMCProperty = ''
PROC.LIST=''
*OLD.PROC.LIST=''
	STATUS=RBO.getProperty('','PMCProperty',PMCProperty)
	STATUS=RBO.getProperty('','UserId',USER.ID)
	STATUS=RBO.getProperty('','Process',PROC.LIST)
*	STATUS=RBO.getProperty('','OLDPROCESS',OLDPROC.LIST)

*PROC.LIST="APCREM":VM:"CUSTI"
SWAP VM WITH AM IN PROC.LIST

CONO = PMCProperty<1,4>
USER.ID=UPCASE(USER.ID)
SEC.ID="_":CONO:USER.ID


	OPEN "","SECURITY" TO SECURITY ELSE
	ERRMSG = "CANNOT OPEN SECURITY FILE"
	GOTO 93000
	END

*	 IF PROC.LIST =OLDPROC.LIST THEN 
	    *   ERRMSG = "THIS PROCESS ALREADY EXIST"
*		GOTO 93000
*	 END

	 IF PROC.LIST = "" THEN
               READ XX FROM SECURITY, SEC.ID THEN
                 DELETE SECURITY, SEC.ID
               END ELSE
                 RELEASE SECURITY, SEC.ID
               END
        END ELSE
	          WRITE PROC.LIST ON SECURITY, SEC.ID
                END 
	RETURN
	93000:
		IF ERRMSG # "" THEN
		      STATUS = RBO.setProperty('', 'ServerStatus', 1)
		      STATUS = RBO.setProperty('', 'ServerMessage', ERRMSG)
	END
	RETURN

	99999:
		RELEASE SECURITY, SEC.ID
	RETURN
