SUBROUTINE JCSMSCE_VALIDATE
********************************************************************************
*   Program name :- JCSMSCE_VALIDATE
*   Created:- 7/16/2003  by RaziMohiuddin
*------------------------------------------------------------------------------*
* In Properties:
* --------------
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE JCS.CPYLIB DAILY.MISC 

ERRMSG=''

OPEN '','DAILY.MISC' TO DAILY.MISC ELSE
   ERRMSG='Cannot open DAILY MISC file!'
   GOTO 1000
END

STATUS = RBO.getProperty('','ID',ID)


MAT DMS.REC = ''
MATREAD DMS.REC FROM DAILY.MISC, ID ELSE
	ERRMSG = ID[4,99] :"  IS NOT ON FILE TRY AGAIN !"
	
GOTO 1000
END 
RETURN    


1000
STATUS = RBO.setProperty('','ServerStatus',1)        
STATUS = RBO.setProperty('','ServerMessage',ERRMSG)

* End of method code
RETURN

