SUBROUTINE JOBM_ESTIMATE_REVIEW_DATA
********************************************************************************
*   Program name :- JOBM_ESTIMATE_REVIEW_DATA
*   Created:- 4/12/2004
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H

* Insert method code here
$INCLUDE JES.CPYLIB ESTIMATE
$INCLUDE PMC.CPYLIB SALESMAN
$INCLUDE JCS.CPYLIB JOB.CATEGORY
$INCLUDE JES.CPYLIB ESTIMATOR

ERRMSG = ''
OPEN '','ESTIMATE' TO EST ELSE
	ERRMSG = "CAN'T OPEN ESTIMATE FILE"
	GOTO 99999
END
OPEN '','JOB.CATEGORY' TO JOB.CATG ELSE
	ERRMSG = "CAN'T OPEN JOB.CATEGORY FILE"
	GOTO 99999
END
OPEN '','SALESMAN' TO SALESMAN ELSE
	ERRMSG = "CAN'T OPEN SALESMAN FILE"
	GOTO 99999
END
OPEN '','ESTIMATOR' TO ESTIMATOR ELSE
	ERRMSG = "CAN'T OPEN ESTIMATOR FILE"
	GOTO 99999
END


VAL = RBO.getProperty('','ID',ID) ;* CONO : ESTIMATE.ID
CONO = ID[1,3]

MATREAD EST.REC FROM EST, ID ELSE
	ERRMSG = "CAN'T FIND RECORD:" : ID[4,99]
	GOTO 99999
END
VAL = RBO.setProperty('','JOB_OP_EXP_DATE',EST.COMMENTS)
VAL = RBO.setProperty('','JOB_OP_LAST_RECV',EST.COMMENT.TYPE)
VAL = RBO.setProperty('','JOB_DIV',EST.DIV)
VAL = RBO.setProperty('','JOB_SUBS',EST.SUBS)
VAL = RBO.setProperty('','JOB_MASTER',EST.MASTER)
VAL = RBO.setProperty('','JOB_DUE_DATE',OCONV(EST.DATE.ENTERED,'D2/'))

MATREAD JCATG.REC FROM JOB.CATG, CONO:EST.CATG ELSE
	ERRMSG = "CAN'T FIND RECORD:" : EST.CATG
	GOTO 99999
END

VAL = RBO.setProperty('','JOB_CATG_DESC',JCATG.DESC)

MATREAD SALESMAN.REC FROM SALESMAN, CONO:EST.SALESMAN ELSE
	ERRMSG = "CAN'T FIND RECORD:" : EST.SALESMAN
	GOTO 99999
END

VAL = RBO.setProperty('','JOB_SALS_NAME',SALS.NAME)

MATREAD ESTIMATOR.REC FROM ESTIMATOR, CONO:EST.ESTIMATOR ELSE
	ERRMSG = "CAN'T FIND RECORD:" : EST.ESTIMATOR
	GOTO 99999
END

VAL = RBO.setProperty('','JOB_EST',ESTMTR.NAME)

*VAL = RBO.setProperty('','JOB_OP_VEND',EST.REC(31))
*VAL = RBO.setProperty('','JOB_DUE_DATE',EST.REC(32))
99999
IF ERRMSG <> '' THEN
	VAL = RBO.setProperty('','ServerStatus','1')
	VAL = RBO.setProperty('','ServerMessage',ERRMSG)
END ELSE
	VAL = RBO.setProperty('','ServerStatus','')
END
* End of method code
RETURN

