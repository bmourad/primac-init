SUBROUTINE GET_DEL_FLAG
********************************************************************************
*   Program name :- GET_DEL_FLAG
*   Created:- 12/3/2004
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE PMC.CPYLIB COMPANY
$INCLUDE GLS.CPYLIB COA.BAL

* Insert method code here
	STATUS = RBO.getProperty('','ID',ACCT.NO)	
	CONO   = ACCT.NO[1,3] 
	OPEN '','COMPANY' TO COMPANY ELSE ERRMSG = 'COMPANY FILE MISSING' ; GOTO 93000   
	OPEN '','CO.COA.BAL' TO CO.COA.BAL ELSE ERRMSG = 'CO.COA.BAL FILE IS MISSING'; GOTO 93000
	MATREAD COMP.REC FROM COMPANY, CONO ELSE MAT COMP.REC = ""

	  IF CO.GLS = 'Y' THEN
            DEL.FLAG = 0
            READ DUMMY FROM CO.COA.BAL, ACCT.NO ELSE DEL.FLAG = 1
         END ELSE
            DEL.FLAG = 1
         END

	  STATUS = RBO.setProperty('','DEL_FLAG',DEL.FLAG)
93000*
IF ERRMSG # "" THEN
     STATUS = RBO.setProperty('','ServerStatus',1)
      STATUS = RBO.setProperty('','ServerMessage',ERRMSG)
END
* End of method code
RETURN

