SUBROUTINE GET_EQUIPFILE_CODES
********************************************************************************
*   Program name :- GET_EQUIPFILE_CODES
*   Created:- 1/30/2006
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE FAS.CPYLIB EQUIP.FILE
$INCLUDE PMC.CPYLIB GLTABLE

OPEN '','EQUIP.FILE' TO EQUIP.FILE ELSE
   ERRMSG = 'EQUIP.FILE FILE IS MISSING'; GOTO 93000
END

OPEN '','CONTROL' TO CONTROL ELSE
   ERRMSG = 'CONTROL FILE IS MISSING'; GOTO 93000
END

DIM EQP_REC(40)
STATUS = RBO.getProperty('','PMCProperty',PMCProperty)
CONO = PMCProperty<1,4>
EquipCodes = ''



CMD = "SSELECT EQUIP.FILE WITH CONO = ":CONO 
UDTEXECUTE CMD
DATA = 1
I = 1
LOOP
	READNEXT ID ELSE DATA = 0
  WHILE DATA DO
       EquipCodes <1,I> = ID[4,99]
  I = I + 1
    
REPEAT

  STATUS = RBO.setProperty('', 'EquipFileCodes', EquipCodes)
 
MATREAD GLTABLE.REC FROM CONTROL,CONO:"GLTABLE" THEN
  STATUS=RBO.setProperty('','GLTB_CASH',GLTB.CASH)
END

RETURN

93000 
  STATUS = RBO.setProperty('','ServerStatus',1)        
  STATUS = RBO.setProperty('','ServerMessage',ERRMSG) 
RETURN



