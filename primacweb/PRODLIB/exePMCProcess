   $INCLUDE PMC.CPYLIB PMCProcessStatus
   OPEN '', 'PMC_REPORT_VALUES' TO PMC_REPORT_VALUES ELSE STOP
   OPEN '', 'PMCProcessStatus' TO PMCProcessStatus ELSE STOP
   PROCREAD IN_BUFF ELSE STOP
   PPSR_ID = IN_BUFF<1>
   TDString = TIMEDATE()
   MATREADU PPSR.REC FROM PMCProcessStatus, PPSR_ID ELSE
   END
   CALL GetSYSVars(PPSR.EXSVAL, TDString)
   MATWRITE PPSR.REC ON PMCProcessStatus, PPSR_ID
   END
