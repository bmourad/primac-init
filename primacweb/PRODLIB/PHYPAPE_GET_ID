SUBROUTINE PHYPAPE_GET_ID
********************************************************************************
*   Program name :- PHYPAPE_GET_ID
*   Created:- 8/26/2003
*------------------------------------------------------------------------------*
*
* AUTHOR G.PURUSHOTHAM RAO
* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE ICS.CPYLIB CATEGORY

OPEN '','CATEGORY' TO CATEGORY ELSE ERRMSG='CATEGORY FILE IS MISSING';GOTO 93000

* Insert method code here
STATUS=RBO.getProperty('','PROD_INV_LINE',PROD.LINE)
STATUS=RBO.getProperty('','PROD_WHSE',WHSE.CODE)
STATUS=RBO.getProperty('','PROD_LOC',LOC.CODE)

STATUS=RBO.getProperty('','PMCProperty',PMCProperty)
CONO = PMCProperty<1,4>
USER.ID = PMCProperty<1,3>

MATREAD CATG.REC FROM CATEGORY,CONO:PROD.LINE ELSE
	MAT CATG.REC = ""
END 
ID = CONO : CATG.TYPE : "!" : PROD.LINE : "!" : WHSE.CODE : "!" : LOC.CODE
STATUS=RBO.setProperty('','ID',ID)

* End of method code
RETURN
93000*
	STATUS = RBO.setProperty("","ServerMessage",ERRMSG)

