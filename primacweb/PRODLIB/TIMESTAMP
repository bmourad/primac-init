SUBROUTINE TIMESTAMP
********************************************************************************
*   Program name :- TIMESTAMP
*   Created:- 6/30/2005
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE CPYLIB CHAR

STATUS=RBO.getProperty('','TIME_CONV_VAL',VALUE)
TEMPVAL=""
CNT=DCOUNT(VALUE,VM)

FOR I=1 TO CNT
  TEMPVAL=VALUE<1,I>
  HH=TEMPVAL[1,2]
  MM=TEMPVAL[3,4]
  TIME.USED<1,I>=ICONV((HH:":":MM:":00"),"MTS")
  IF LEN(TIME.USED<1,I>)=3 THEN
     TIME.USED<1,I>=00:TIME.USED<1,I>
  END
  IF LEN(TIME.USED<1,I>)=4 THEN
     TIME.USED<1,I>=0:TIME.USED<1,I>
  END
 
NEXT I
STATUS=RBO.setProperty('','FINAL_CONV_VAL',TIME.USED)

RETURN

