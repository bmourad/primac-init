SUBROUTINE VALIDATE_LABEL
********************************************************************************
*   Program name :- VALIDATE_LABEL
*   Created:- 7/20/2007
*------------------------------------------------------------------------------*
*

* In Properties:
* --------------
*  
*
* Out Properties:
* ---------------
*  
********************************************************************************
$INCLUDE WWINSERT RBO.H
$INCLUDE POS.CPYLIB RS.LABEL
* Insert method code here
ERRMSG = ""; REC = ""
OPEN "","RS.LABEL" TO RS.LABEL ELSE
    ERRMSG = "CANNOT OPEN RS.LABEL FILE"
    GOTO 93000
END

STATUS = RBO.getProperty('','ID',ID)
READ ID FROM RS.LABEL,ID  ELSE ERRMSG = ID[4,99]:" item ID missing from RS.LABEL";	GOTO 93000
	REC = ID[4,99]
	STATUS = RBO.setProperty('','LABEL',REC)
* End of method code
RETURN
93000 *
IF  ERRMSG # '' THEN
    STATUS = RBO.setProperty('','ServerStatus',1)
    STATUS = RBO.setProperty('','ServerMessage',ERRMSG)
END
RETURN
