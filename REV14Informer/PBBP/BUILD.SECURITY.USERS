OPEN '','SECURITY' TO SECURITY ELSE STOP
OPEN '','SECURITY.USERS' TO SECURITY.USERS ELSE STOP
READ MREC FROM SECURITY.USERS, "security_admin" ELSE STOP
PERFORM 'SELECT SECURITY WITH CONO = "001"'
DATA = 1
LOOP
  READNEXT ID ELSE DATA = 0
WHILE DATA DO
  REC = MREC
  READ SREC FROM SECURITY, ID THEN
    REC<1> = SREC<10>
    REC<10> = SREC<1>
    WRITE REC ON SECURITY.USERS, ID[4,99]
  END
REPEAT
END
