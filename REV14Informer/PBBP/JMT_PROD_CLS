SUBROUTINE JMT_PROD_CLS(CLS,CONO,PROD)
INV_LINE = OCONV(CONO:PROD,"TINVENTORY;X;;5")
CATG.TYPE = OCONV(CONO:INV_LINE,"TCATEGORY;X;;8")
BEGIN CASE
   CASE CATG.TYPE = "P"
      CLS = "MP"
   CASE CATG.TYPE = "I"
      CLS = "MI"
   CASE CATG.TYPE = "F"
      CLS = "MF"
   CASE CATG.TYPE = "O"
      CLS = "MO"
   CASE CATG.TYPE = "S"
      CLS = "MS"
   CASE 1
      CLS = "MR"
END CASE
RETURN
END
