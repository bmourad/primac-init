SUBROUTINE JBS_CUR_CCTR (ACT_CCTR,JBS_ID)
*COPY>PSS.CPYLIB>JOB.SCHED
ACT_CCTR =  " "
FND = 0
OPEN '','JOB.SCHED' TO JOB.SCHED ELSE RETURN
MATREAD JBS.REC FROM JOB.SCHED, JBS_ID ELSE RETURN
10*
CCTR.CNT = DCOUNT(JBS.CCTR.STATUS,@VM)
FOR X = 1 TO CCTR.CNT
   IF JBS.CCTR.STATUS<1,X> = "I" THEN ACT_CCTR = JBS.CCTR<1,X>; FND = 1
NEXT X
IF FND = 0 THEN ACT_CCTR = " "
RETURN
