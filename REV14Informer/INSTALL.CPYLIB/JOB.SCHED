EQU JBS.COMPL.DATE    TO JBS.REC(3) ;* Completion date
EQU JBS.SCH.FLAG      TO JBS.REC(4) ;*
* T22896                            ;* "R" = Chge to Job or Estimate
* T22896                            ;* "P" = New Job First Time
* T22896                            ;* "U" = Estimate removed from job
* T22896                            ;* "S" = Transfered to scheduling
EQU JBS.CUST.NAME     TO JBS.REC(5) ;* Customer Name
EQU JBS.DIV           TO JBS.REC(6) ;* Division ID
EQU JBS.EXT.PRIOR     TO JBS.REC(7) ;* External priority
