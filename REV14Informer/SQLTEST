CREATE VIEW SQLTEST_1 AS SELECT @ID,
                            VPDS_REC_DATE,VPDS_REC_QTY,
VPDS_REC_UN_COST,VPDS_REC_FI_DATE,
VPDS_REC_DATE_MC,VPDS_REC_M_WT,VPDS_REC_PROD,
VPDS_REC_WTH,VPDS_REC_QTY_FLT,VPDS_REC_QTY_FT,
VPDS_REC_QTY_PC,VPDS_REC_QTY_SHT FROM VEND_PROD_STATS UNNEST NL_ALL  VPDS_REC_DATE,VPDS_REC_QTY,VPDS_REC_UN_COST,
VPDS_REC_FI_DATE,VPDS_REC_DATE_MC,VPDS_REC_M_WT,
                               VPDS_REC_PROD,VPDS_REC_WTH,
VPDS_REC_QTY_FLT,VPDS_REC_QTY_FT,VPDS_REC_QTY_PC,
VPDS_REC_QTY_SHT;                        
