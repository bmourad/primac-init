*****************************
*  SIS_LOG FILE DEFINITION  *
*      KEY = CONO:EMP       *
*****************************
DIM SISLOG_REC(25)
EQU SISLOG_DIV              TO SISLOG_REC(1) ;* Logon division
EQU SISLOG_DEPT             TO SISLOG_REC(2) ;* Logon department
EQU SISLOG_CCTR             TO SISLOG_REC(3) ;* Logon costcenter
EQU SISLOG_SHIFT            TO SISLOG_REC(4) ;* Logon shift
EQU SISLOG_SERVER           TO SISLOG_REC(5) ;* Logon server
EQU SISLOG_CLIENT           TO SISLOG_REC(6) ;* Logon client
EQU SISLOG_DATE             TO SISLOG_REC(7) ;* Logon date (date worked)
EQU SISLOG_TIME             TO SISLOG_REC(8) ;* Logon time
EQU SISLOG_LAST.SERVER      TO SISLOG_REC(9) ;* Server of last trans
EQU SISLOG_LAST.CLIENT      TO SISLOG_REC(10);* Client of last trans
EQU SISLOG_LAST.DATE        TO SISLOG_REC(11);* Date of last trans
EQU SISLOG_LAST.TIME        TO SISLOG_REC(12);* Time of last trans
*
EQU SISLOG_WCTR             TO SISLOG_REC(13);* (MV2) Cost center ID
EQU SISLOG_WCTR.DATE        TO SISLOG_REC(14);* (MV2) Last date for CCTR
EQU SISLOG_WCTR.TIME        TO SISLOG_REC(15);* (MV2) Last time for CCTR
EQU SISLOG_WCTR.TIMESTMP    TO SISLOG_REC(16);* (MV2,MSV) CCTR time stamp
*
EQU SISLOG_OFF.FLAG         TO SISLOG_REC(20);* A=auto, U=user
EQU SISLOG_OFF.DATE         TO SISLOG_REC(21);* Logoff Date
EQU SISLOG_OFF.TIME         TO SISLOG_REC(22);* Logoff Time
EQU SISLOG_DATE.OFFSET      TO SISLOG_REC(23);* Login offset -1,0,1
