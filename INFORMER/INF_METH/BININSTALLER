SUBROUTINE BININSTALLER(PROG.NAME, OBJ.CODE)

NOCONVERT ON
OBJ = ""
SRC = ""
CNT = DCOUNT(OBJ.CODE,":")
FOR X = 1 TO CNT
   NUM = FIELD(OBJ.CODE,":",X)
   IF NUM = -125 THEN NUM = 131
   OBJ = OBJ:CHAR(NUM)
   SRC<X> = NUM
NEXT X

OSWRITE OBJ ON "INF_METH/":PROG.NAME
OSWRITE "" ON "INF_METH/":PROG.NAME[2,99]

END
