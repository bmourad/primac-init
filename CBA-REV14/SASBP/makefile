
#
#  Make file for BP's in SASBP
#

BAS = BASIC 
BAS.OPT =
CAT = CATALOG
CAT.OPT = FORCE DIRECT
LOCAL.CAT.OPT = FORCE LOCAL 
BP = SASBP
MK = ../SAVEDLISTS/SASBP

ALL: _JOB.SALES.MAINT _JOB.STATS.SUM _JOB.STATS.SUM.DEL _JOB.STATS.SUM.RANK _JOB.STATS.SUM.RPT _SAS.REPORT.COLOR _SAS.REPORT.CUST _SAS.REPORT.DVAL _SAS.REPORT.SCODE _SAS.REPORT.SLSM _SAS.REPORT.TERR _SAS.REPORT.TYPE 


_JOB.SALES.MAINT: JOB.SALES.MAINT 
	echo "$(BAS) $(BP) 'JOB.SALES.MAINT' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'JOB.SALES.MAINT' $(CAT.OPT) " >> $(MK)

_JOB.STATS.SUM: JOB.STATS.SUM 
	echo "$(BAS) $(BP) 'JOB.STATS.SUM' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'JOB.STATS.SUM' $(CAT.OPT) " >> $(MK)

_JOB.STATS.SUM.DEL: JOB.STATS.SUM.DEL 
	echo "$(BAS) $(BP) 'JOB.STATS.SUM.DEL' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'JOB.STATS.SUM.DEL' $(CAT.OPT) " >> $(MK)

_JOB.STATS.SUM.RANK: JOB.STATS.SUM.RANK 
	echo "$(BAS) $(BP) 'JOB.STATS.SUM.RANK' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'JOB.STATS.SUM.RANK' $(CAT.OPT) " >> $(MK)

_JOB.STATS.SUM.RPT: JOB.STATS.SUM.RPT 
	echo "$(BAS) $(BP) 'JOB.STATS.SUM.RPT' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'JOB.STATS.SUM.RPT' $(CAT.OPT) " >> $(MK)

_SAS.REPORT.COLOR: SAS.REPORT.COLOR 
	echo "$(BAS) $(BP) 'SAS.REPORT.COLOR' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'SAS.REPORT.COLOR' $(CAT.OPT) " >> $(MK)

_SAS.REPORT.CUST: SAS.REPORT.CUST 
	echo "$(BAS) $(BP) 'SAS.REPORT.CUST' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'SAS.REPORT.CUST' $(CAT.OPT) " >> $(MK)

