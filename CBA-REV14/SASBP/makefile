	echo "$(BAS) $(BP) 'SAS.REPORT.DVAL' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'SAS.REPORT.DVAL' $(CAT.OPT) " >> $(MK)

_SAS.REPORT.SCODE: SAS.REPORT.SCODE 
	echo "$(BAS) $(BP) 'SAS.REPORT.SCODE' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'SAS.REPORT.SCODE' $(CAT.OPT) " >> $(MK)

