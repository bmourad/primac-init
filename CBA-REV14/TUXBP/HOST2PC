 INCLUDE TUBP TU.API.H
 INCLUDE TUBP SPECIAL.H
 INCLUDE TUBP ROC.H
 INCLUDE TUBP USER.INCLUDE.H
*
ERROR = FALSE
*
 INCLUDE TUBP CASE.INVERT.OFF.H
*
CALL TU.INIT(ERROR)
IF ERROR THEN CRT 'ERROR! Cannot Init'; GO 999
PARENT = NUL
CALL ROC.GET("this", "parent", PARENT, ERROR)
HSESS = PARENT
CALL ROC.GET(PARENT, "mdi_client_form", PARENT, ERROR)
IF NOT(ERROR) THEN
ATTS = "rpc_startup"
VALS = "host2pcclass"
NAME = "HOST2PC":HSESS
VALS<2> = "parent":VM:"session_handle":VM:"run":VM:"object_name"
VALS<3> = PARENT :VM:HSESS :VM:TRUE :VM:NAME
CALL ROC.MAIN(ROC.SERVE, "this", ATTS, VALS, NUL, ERROR)
IF NOT(ERROR) THEN
VALS = ''
CALL ROC.GETHANDLE(NAME, VALS, ERROR)
IF VALS # NUL THEN CALL ROC.DESTROY(VALS, ERROR)
END
END
999 *
IF PTERM THEN EXECUTE "PTERM CASE INVERT"
END
