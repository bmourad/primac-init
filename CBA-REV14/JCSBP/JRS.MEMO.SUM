   SUBROUTINE JRS.MEMO.SUM
*COPY>JCS.CPYLIB>COM.JRS
*******************************************************************
* REVISION    - [12.0]
* COPYRIGHT   - 1982 by C.B.A.  (Vercom Software, Inc.)
* SYSTEM      - PRIMAC
* SOURCE      - JCSBP
* PROGRAM     - JRS.MS.SUM
* BY          - ABDULLAH JIBALY, C.B.A.
* DATE        - 07/27/01
* DESCRIPTION - Program produces a CSR Memo list (TASK 25967)
*T26556 ajibaly 05/09/2002 * USE REPORT NO.
*T26731 adelgado 07/08/2002 * Allow to print multiple CSR Memos tied to
*                             a job.
*ENDDOC
*******************************************************************
*
*-- FILE EQUATES
*
*COPY>PMC.CPYLIB>CSR.MEMO.TRACK
*COPY>PMC.CPYLIB>DEPARTMENT
*COPY>JCS.CPYLIB>JOB
*COPY>JCS.CPYLIB>OPERATION
*COPY>JCS.CPYLIB>JOB.RPT.SET
*COPY>JCS.CPYLIB>JOB.RPT.SUM
*COPY>CPYLIB>FILE.VARS
*COPY>CPYLIB>CHAR
*
*-- INITIALIZATION
*
*T26556 v
* RPT.NAME = "CSR.MEMO.TRACKING"
* RPT.NO = "XXXX"
   RPT.NAME = ""
*T26556 ^
   RPT.DATE = DATE()
   HD1=''; HD2=''
   CALL GET.PROG.HEAD(CONO,C.CO.NAME,RPT.NAME,RPT.NO,RPT.DATE,HD1,HD2)
  * T26731 v
  * STMT = "LIST CSR.MEMO.TRACK '":CONO:JOBNO"R%8":"' BREAK.SUP ":DQUOTE("'B'")
   STMT = 'SORT CSR.MEMO.TRACK WITH CONO = "':CONO:'" AND WITH JOB.NO = "':JOBNO:'" '
  * T26731 ^
   STMT:= " MEMO.NUM CSR.MEMO CSR.RESP RESP.DATES ID-SUP HEADING "
  * T26731 v
  * STMT:= DQUOTE("'N'":HD1:"'PL'":HD2:"'L'JOB #    :":JOBNO:"'LL'LISTING MEMO 'B'")
   STMT:= DQUOTE("'N'":HD1:"'PL'":HD2:"'L'JOB #    :":JOBNO:"'LL'")
  * T26731 ^
   STMT:= " COUNT.SUP LPTR"
   PRINTER CLOSE ;* FORCE ALL PRIOR PRINT JOBS TO GO FIRST
   UDTEXECUTE STMT CAPTURING JUNK RETURNING ERRORS
99999*
   RETURN ; * C42403
END
