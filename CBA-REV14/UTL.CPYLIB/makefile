
#
#  Make file for BP's in UTL.CPYLIB
#

BAS = BASIC 
BAS.OPT =
CAT = CATALOG
CAT.OPT = FORCE DIRECT
LOCAL.CAT.OPT = FORCE LOCAL 
BP = UTL.CPYLIB
MK = ../SAVEDLISTS/UTL.CPYLIB

ALL: _BUILD.PMC.Sa _COM.BUILD.Pa _COM.INS.PMCa _COM.INS.PMCb _COM.SYSTEM _INS.PMC.MAIa _INS.PMC.SYS _STAT-FILE _SYS.FILES _SYS.MENUS _SYS.PROCS _SYS.PROGRAMa _SYS.SCREENS _SYSTEM.MAINT _SYSTEM.VARS _TAPE.FILE _TAPE.LABEL 


_BUILD.PMC.Sa: BUILD.PMC.Sa 
	echo "$(BAS) $(BP) 'BUILD.PMC.Sa' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'BUILD.PMC.Sa' $(CAT.OPT) " >> $(MK)

_COM.BUILD.Pa: COM.BUILD.Pa 
	echo "$(BAS) $(BP) 'COM.BUILD.Pa' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'COM.BUILD.Pa' $(CAT.OPT) " >> $(MK)

_COM.INS.PMCa: COM.INS.PMCa 
	echo "$(BAS) $(BP) 'COM.INS.PMCa' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'COM.INS.PMCa' $(CAT.OPT) " >> $(MK)

_COM.INS.PMCb: COM.INS.PMCb 
	echo "$(BAS) $(BP) 'COM.INS.PMCb' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'COM.INS.PMCb' $(CAT.OPT) " >> $(MK)

_COM.SYSTEM: COM.SYSTEM 
	echo "$(BAS) $(BP) 'COM.SYSTEM' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'COM.SYSTEM' $(CAT.OPT) " >> $(MK)

_INS.PMC.MAIa: INS.PMC.MAIa 
