_SYS.MENUS: SYS.MENUS 
	echo "$(BAS) $(BP) 'SYS.MENUS' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'SYS.MENUS' $(CAT.OPT) " >> $(MK)

_SYS.PROCS: SYS.PROCS 
	echo "$(BAS) $(BP) 'SYS.PROCS' $(BAS.OPT)" >> $(MK)
