	echo "$(CAT) $(BP) 'INS.PMC.SYS' $(CAT.OPT) " >> $(MK)

_STAT-FILE: STAT-FILE 
	echo "$(BAS) $(BP) 'STAT-FILE' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'STAT-FILE' $(CAT.OPT) " >> $(MK)

