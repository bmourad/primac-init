UDTEXECUTE 'CLEAR.FILE INV_AUDIT_HIST'
UDTEXECUTE 'CLEAR.FILE INV_AUDIT_BAL'
UDTEXECUTE 'DELETE CONTROL "001INV.AUDIT.HIST"'
*COPY>ICS.CPYLIB>INV_RECEIPTS
*COPY>ICS.CPYLIB>INV_SERIAL
*COPY>ICS.CPYLIB>INV.WHSE
OPEN '','INV_RECEIPTS' TO INV_RECEIPTS ELSE STOP 201
OPEN '','INV_SERIAL' TO INV_SERIAL ELSE STOP 201
OPEN '','INV.WHSE' TO INV.WHSE ELSE STOP 201
SELECT INV_RECEIPTS
*UDTEXECUTE "GET.LIST EP3"
DONE=0
LOOP
  READNEXT INVR.ID ELSE DONE=1
UNTIL DONE DO
  MATREAD INVR.REC FROM INV_RECEIPTS,INVR.ID THEN
    INVR.AUDIT.NO=''
    MATWRITE INVR.REC ON INV_RECEIPTS,INVR.ID
  END
REPEAT
*UDTEXECUTE 'GET.LIST EP2'
SELECT INV_SERIAL
DONE=0
LOOP
  READNEXT ISTK.ID ELSE DONE=1
UNTIL DONE DO
  MATREAD ISTK.REC FROM INV_SERIAL,ISTK.ID THEN
    ISTK.AUDIT.NO=''
    MATWRITE ISTK.REC ON INV_SERIAL,ISTK.ID
  END
REPEAT
SELECT INV.WHSE
DONE=0
LOOP
  READNEXT IWH.ID ELSE DONE=1
UNTIL DONE DO
  MATREAD IWH.REC FROM INV.WHSE,IWH.ID THEN
    IWH.BAL.PERIOD=''
    IWH.BEG.QTY=''
    IWH.BEG.AMT=''
    MATWRITE IWH.REC ON INV.WHSE,IWH.ID
  END
REPEAT
