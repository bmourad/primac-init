SUBROUTINE CONV.RPT (ID)
OPEN '','CONVERSION.ERRORS' TO CONVERSION.ERRORS ELSE STOP 201
READ REC FROM CONVERSION.ERRORS,ID THEN 
  PRINTER ON                            
PRINT
PRINT ID
PRINT
  CNT=DCOUNT(REC,@AM)                   
  FOR LN=1 TO CNT                       
    VCNT=DCOUNT(REC<LN>,@VM)            
    FOR V=1 TO VCNT                     
      PRINT REC<LN,V>                   
    NEXT V                              
  NEXT LN                               
END                                     
PRINTER OFF ; PRINTER CLOSE             
