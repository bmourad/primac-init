OPEN '','INV.WHSE' TO INV.WHSE ELSE STOP 201
*COPY>ICS.CPYLIB>INV.WHSE
SELECT INV.WHSE
DEBUG
EOL=0
LOOP
  READNEXT ID ELSE EOL=1
UNTIL EOL DO
  MATREADU IWH.REC FROM INV.WHSE,ID THEN
    LCNT=DCOUNT(IWH.LOC,@VM)
    FOR L=LCNT TO 1  STEP -1
      IF IWH.LOC<1,L>='' THEN
        DEL IWH.LOC<1,L>
      END
    NEXT L
    MATWRITE IWH.REC ON INV.WHSE,ID
  END
REPEAT
