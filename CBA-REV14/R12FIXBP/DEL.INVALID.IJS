OPEN '','INV.JOB.STATS' TO INV.JOB.STATS ELSE STOP 201
OPEN '','JOB' TO JOB ELSE STOP 201
*
*COPY>ICS.CPYLIB>INV.JOB.STATS
*COPY>JCS.CPYLIB>JOB
*
SELECT INV.JOB.STATS
DONE=0
LOOP
  READNEXT IJS.ID ELSE DONE=1
UNTIL DONE DO
  CONO=IJS.ID[1,3]
  JOBNO=OCONV(IJS.ID,'G2!1')
  READ REC FROM JOB,CONO:JOBNO ELSE
*    DELETE INV.JOB.STATS,IJS.ID
PRINT IJS.ID
  END
REPEAT
