OPEN "INV_AUDIT_BAL" TO INV.AUDIT.BAL ELSE STOP
*COPY>ICS.CPYLIB>INV_AUDIT_BAL
MAX = 8
BASE = 200501
IF @LOGNAME = 'cba12' THEN DEBUG
100 *
READNEXT ID ELSE GO 9999
OLDID = "001":ID:"!":BASE
MATREAD ITB.REC FROM INV.AUDIT.BAL,OLDID ELSE GO 500
FOR X = 2 TO MAX 
      BASE = BASE + 1
  NEW.ID = "001":OLDID:"!":BASE
  MATWRITE ITB.REC ON INV.AUDIT.BAL, NEW.ID
NEXT X
500 *
GO 100
*
9999 *
END
