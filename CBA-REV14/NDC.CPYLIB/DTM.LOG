***********************************
* REVISION  - [10.2]              *
* DTM.LOG FILE DEFINITION         *
* KEY = CONO:SEQ                  *
***********************************
DL.REC.SIZE = (20)
DIM DL.REC(20)
EQU DL.DATE          TO DL.REC(1)     ;* Date
EQU DL.EMP           TO DL.REC(2)     ;* Employee
EQU DL.SHIFT         TO DL.REC(3)     ;* Shift
EQU DL.USER          TO DL.REC(4)     ;* User name
EQU DL.ACTION        TO DL.REC(5)     ;* ADD,DEL,CHG
EQU DL.TYPE          TO DL.REC(6)     ;* (MV1) Type (T,M,L,J)
EQU DL.JOB           TO DL.REC(7)     ;* (MV1) Job number
EQU DL.CCTR          TO DL.REC(8)     ;* (MV1) Cost center worked
EQU DL.OPER          TO DL.REC(9)     ;* (MV1) Operation performed
EQU DL.JOB.TYPE      TO DL.REC(10)    ;* (MV1) Job type (R,C,S,N)
EQU DL.TIME.ST       TO DL.REC(11)    ;* (MV1) Start time (HHMM)
EQU DL.TIME          TO DL.REC(12)    ;* (MV1) End time (HHMM)
EQU DL.IMP           TO DL.REC(13)    ;* (MV1) Production quantity
EQU DL.PROD          TO DL.REC(14)    ;* (MV1) Inventory product ID
EQU DL.QTY           TO DL.REC(15)    ;* (MV1) Inventory quantity used
EQU DL.WHSE          TO DL.REC(16)    ;* (MV1) Inventory warehouse
EQU DL.LOC           TO DL.REC(17)    ;* (MV1) Inventory location
EQU DL.R.S.ID        TO DL.REC(18)    ;* (MV1) Roll or skid ID
EQU DL.OPER.COMPLETE TO DL.REC(19)    ;* (MV1) Operation Complete
