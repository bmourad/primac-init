SUBROUTINE TESTFOLDER_TAXJM_GETVALUES(CONO,KEY,SCHEMA.ONLY, ERRMSG, STRXML)
STRXML = "<Xmldata>"
*STRXML = '<TAX INFO><TAXID></TAXID><TAX_DESCRIPTION></TAX_DESCRIPTION><TAX-ACCT></TAX-ACCT><TAX-RATE></TAX-RATE></TAX INFO>'
PRINT @(-1)
$INCLUDE PMC.CPYLIB TAX
$INCLUDE PMC.CPYLIB COMPANY
$INCLUDE PMC.CPYLIB COA
OPEN "","TAX" TO TAX ELSE
   ERRMSG = "CANNOT OPEN TAX FILE"
   RETURN
END
OPEN "","COMPANY" TO COMPANY ELSE
   ERRMSG = "CANNOT OPEN COMPANY FILE"
   RETURN
END
OPEN "","COA" TO COA ELSE
   ERRMSG = "CANNOT OPEN COA FILE"
   RETURN
END
MATREAD TAX.REC FROM TAX, CONO:KEY THEN
*PRINT @(5,21):" IF IS ":KEY ; INPUT SS
**PRINT @(5,21):" ID IS ":KEY ; INPUT SS
*STRXML :='<TAX INFO>'
STRXML := '<TAXID>' : KEY : '</TAXID>'
STRXML := '<TAX-DESCRIPTION>' : TAX.JUR : '</TAX-DESCRIPTION>'
STRXML := '<TAX-ACCOUNT>' : TAX.ACCT : '</TAX-ACCOUNT>'
STRXML := '<TAX-RATE>' : TAX.RATE : '</TAX-RATE>'
*STRXML := '</TAX INFO>'
STRXML := "</Xmldata>"
END ELSE
 ERRMSG = "THE TAX ID " :KEY:" IS NOT IN TAX FILE"
END
RETURN
