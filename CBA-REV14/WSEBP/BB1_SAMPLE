SUBROUTINE BB_SAMPLE(CONO, ID, SCHEMA.ONLY, ERRMSG, STRXML)
STRXML := '<TableName>....</TableName>'
$INCLUDE PMC.CPYLIB COMPANY
$INCLUDE CPYLIB CHAR
OPEN '','FileName' to FILENAME ELSE
ERRMSG = 'CANNOT OPEN FILENAME FILE'
 GOTO 99999
 GOTO 99999
END
OPEN '','COMPANY' TO COMPANY ELSE
 ERRMSG ='CANNOT OPEN COMPANY FILE'
 GOTO 99999
END
SCHEMA.ONLY="1"
 MATREAD COMP.REC FROM COMPANY,CONO ELSE
    ERRMSG=CONO:"- THIS COMPANY NUMBER DOES NOT EXIST"
    GOTO 99999
 END
MATREAD FILENAME.REC FROM FILENAME, CONO:FILENAME.ID THEN
  STRMXL=''
  SCHEMA.ONLY ="0"
  STRXML := '<TableName>'
  STRXML := '<VariableName>' : VARIABLENAME : '</VariableName>'
...
  STRXML := '</TabeName>'
END
99999*
IF ERRMSG <> "" THEN
ERRMSG = "BB_Sample  ---> BB_SAMPLE ---> " : ERRMSG
CALL WRITELOG(ERRMSG)
RETURN
END
