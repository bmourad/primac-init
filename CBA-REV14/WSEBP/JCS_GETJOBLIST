SUBROUTINE JCS_GETJOBLIST(CONO, SCHEMA.ONLY, ERRMSG, STRXML)
STRXML = '<JobList><JobId></JobId></JobList>'
CMD= "SSELECT JOB WITH @ID LIKE '" : CONO : "...'"
UDTEXECUTE CMD
DATA=1
LOOP
  READNEXT ID ELSE DATA=0
  WHILE DATA DO
    STRXML := '<JobList><JobId>' : ID[4,99] : '</JobId></JobList>'
REPEAT
RETURN
