SUBROUTINE JES_WRITEEST(INPUT_VALUE,CHK,EST_ID)
$INCLUDE PMC.CPYLIB COMPANY
$INCLUDE JES.CPYLIB ESTIMATE
 ERRMSG=""
 WEBSERVICE=""
OPEN "COMPANY" TO COMPANY ELSE ERRMSG="CANNOT OPEN COMPANY FILE";GOTO 99999
OPEN "CONTROL" TO CONTROL ELSE ERRMSG="CANNOT OPEN CONTROL FILE";GOTO 99999
EST_ID=""
READU NUM FROM CONTROL,CONO:"EST.SEQ.NUM" ELSE NUM=10001
SEQ="01"
EST.KEY=NUM:"-":SEQ
       EST.SEQ = EST.KEY
*      EST.DUP.KEY=EST.KEY
       NUM=NUM+1
       WRITE NUM ON CONTROL,CONO:"EST.SEQ.NUM"
99999*
 IF ERRMSG <> "" THEN
ERRMSG = WEBSERVICE:"---> JCS_WRITEJOB---> " : ERRMSG
CALL WRITELOG(ERRMSG)
END
RETURN
