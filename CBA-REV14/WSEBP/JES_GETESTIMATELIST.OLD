SUBROUTINE JES_GETESTIMATELIST(CONO, SCHEMA.ONLY, ERRMSG, STRXML)
SCHEMA.ONLY = 1
STRXML = '<Estimate></Estimate>'
CNT = 0
CMD= "SSELECT ESTIMATE WITH @ID LIKE '" : CONO : "...'"
UDTEXECUTE CMD
DATA=1
LOOP
  READNEXT ID ELSE DATA=0
  WHILE DATA DO
    SCHEMA.ONLY = 0
    CNT = CNT + 1
    IF CNT = 1 THEN
      STRXML = '<Estimate>' : ID[4,99] : '</Estimate>'
    END ELSE
      STRXML := '<Estimate>' : ID[4,99] : '</Estimate>'
    END
REPEAT
RETURN
