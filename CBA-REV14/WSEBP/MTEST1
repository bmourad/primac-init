  OPEN "","CONTROL" TO CONTROL ELSE
          ERRMSG = "CANNOT OPEN CONTROL FILE"
    RETURN
  END
 EMP_ID="1000"
 CONO="001"
 TRANSIT_ID="0000056"
 WARE_HOUSE="02"
 LOCATION="01"
 SERIAL_ID="00114959"
 CORRECT="Y"
 ERRMSG=""
 CALL  RFT_WRITE.RCV.RF.WHSE.RECV(CONO,EMP_ID,TRANSIT_ID,WARE_HOUSE,LOCATION,SERIAL_ID,CORRECT,ERRMSG)
 IF ERRMSG # "" THEN
  WRITE ERRMSG TO CONTROL,"TEST1231"
 END ELSE
  WRITE STRXML TO CONTROL,"TEST12341"
 END
 RETURN
