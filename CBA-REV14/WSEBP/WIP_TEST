OPEN "","CONTROL" TO CONTROL ELSE
 ERRMSG = "CANNOT OPEN CONTROL FILE"
RETURN
END
CONO="001"
OPTIONVALUE ="1"
EMPLOYEEID = "1000"
PALLETID = ""
LOCATION =""
UPDATEFLAG =""
U_JOBID =""
U_FORM =""
U_SKID =""
U_TOCCTR =""
QTY_FLAG =""
U_QTY =""
RANGE_FG =""
U_CARTTONS =""
PERMS_ADJ_FLG =""
U_ADJFLAG =""
STRXML =""
ERRMSG =""
CALL  RFT_WIP_TRACKING(CONO,OPTIONVALUE,EMPLOYEEID,PALLETID,LOCATION,UPDATEFLAG,U_JOBID,U_FORM,U_SKID,U_TOCCTR,QTY_FLAG,U_QTY,RANGE_FG,U_CARTTONS,PERMS_ADJ_FLG,U_ADJFLAG,STRXML,ERRMSG)
IF ERRMSG # "" THEN
  WRITE ERRMSG TO CONTROL,"TEST123"
END
RETURN
