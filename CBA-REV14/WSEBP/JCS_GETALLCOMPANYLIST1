SUBROUTINE JCS_GETALLCOMPANYLIST1(OUTPUTXML,SCHEMA.ONLY)
OUTPUTXML ='<Company><Cono></Cono><DivisionId></DivisionId><DivisionName></DivisionName></Company>'
SCHEMA.ONLY = 1
ERRMSG=""
 CNT = 1
$INCLUDE PMC.CPYLIB COMPANY
$INCLUDE PMC.CPYLIB DIVISION
 OPEN "COMPANY" TO COMPANY ELSE ERRMSG="CANNOT OPEN COMPANY FILE";GOTO 99999
OPEN "DIVISION" TO DIVISION ELSE ERRMSG="CANNOT OPEN DIVISION FILE";GOTO 99999
 CMD= "SSELECT COMPANY"
 UDTEXECUTE CMD
