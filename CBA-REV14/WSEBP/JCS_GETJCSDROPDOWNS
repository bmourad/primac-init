SUBROUTINE JCS_GETJCSDROPDOWNS(CONO, OUTPUTXML, SCHEMA.ONLY, ERRMSG)
  OUTPUTXML = '<EstimateData>'
  TMP.OUTPUT = ''
  CALL JES_GETESTIMATELIST(CONO, SCHEMA.ONLY, ERRMSG, TMP.OUTPUT)
  OUTPUTXML := TMP.OUTPUT : '</EstimateData>'
  OUTPUTXML := '<CustomerData>'
  TMP.OUTPUT = ''
  CALL JCS_GETCUSTOMERLIST(CONO, TMP.OUTPUT, SCHEMA.ONLY)
  OUTPUTXML := TMP.OUTPUT : '</CustomerData>'
  OUTPUTXML := '<DivisionData>'
  TMP.OUTPUT = ''
  CALL JCS_GETDIVISION(CONO, TMP.OUTPUT, SCHEMA.ONLY)
  OUTPUTXML := TMP.OUTPUT : '</DivisionData>'
  OUTPUTXML := '<SalesRepData>'
  TMP.OUTPUT = ''
  CALL JCS_GETSALESREPLIST(CONO, TMP.OUTPUT, SCHEMA.ONLY)
  OUTPUTXML := TMP.OUTPUT : '</SalesRepData>'
  OUTPUTXML := '<CsrData>'
  TMP.OUTPUT = ''
  CALL JCS_GETCSRLIST(CONO, TMP.OUTPUT, ERRMSG, SCHEMA.ONLY)
  OUTPUTXML := TMP.OUTPUT : '</CsrData>'
  OUTPUTXML := '<SalesCodeData>'
  TMP.OUTPUT = ''
  CALL JCS_GETSALESCODES(CONO, TMP.OUTPUT, SCHEMA.ONLY)
  OUTPUTXML := TMP.OUTPUT : '</SalesCodeData>'
  OUTPUTXML := '<JobCategoriesData>'
  TMP.OUTPUT = ''
  CALL JCS_GETJOBCATEGORIES(CONO, TMP.OUTPUT, SCHEMA.ONLY)
  OUTPUTXML := TMP.OUTPUT : '</JobCategoriesData>'
  OUTPUTXML := '<ShipViaData>'
  TMP.OUTPUT = ''
  CALL JCS_GETSHIPVIALIST(CONO, TMP.OUTPUT, SCHEMA.ONLY)
  OUTPUTXML := TMP.OUTPUT : '</ShipViaData>'
  OUTPUTXML := '<JobAllocationTableData>'
  TMP.OUTPUT = ''
  CALL JCS_GETJOBALLOCATIONTABLE(CONO, SCHEMA.ONLY, ERRMSG, TMP.OUTPUT)
  OUTPUTXML := TMP.OUTPUT : '</JobAllocationTableData>'
  OUTPUTXML := '<UOMData>'
  TMP.OUTPUT = ''
  CALL JCS_GETINVENTORYUOM(CONO, ERRMSG, TMP.OUTPUT,  SCHEMA.ONLY)
  OUTPUTXML := TMP.OUTPUT : '</UOMData>'
  OUTPUTXML := '<TermsData>'
  TMP.OUTPUT = ''
  CALL JCS_GETTERMSCODE(CONO, SCHEMA.ONLY, ERRMSG, TMP.OUTPUT)
  OUTPUTXML := TMP.OUTPUT : '</TermsData>'
  OUTPUTXML := '<InventoryData>'
  TMP.OUTPUT = ''
  CALL ICS_GETPRODUCTLIST(CONO, SCHEMA.ONLY, ERRMSG, TMP.OUTPUT)
  OUTPUTXML := TMP.OUTPUT : '</InventoryData>'
  OUTPUTXML := '<InventoryWarehouseData>'
  TMP.OUTPUT = ''
  CALL ICS_GETWAREHOUSELIST(CONO, SCHEMA.ONLY, ERRMSG, TMP.OUTPUT)
  OUTPUTXML := TMP.OUTPUT : '</InventoryWarehouseData>'
  OUTPUTXML := '<JobListData>'
  TMP.OUTPUT = ''
  CALL JCS_GETJOBLIST(CONO, SCHEMA.ONLY, ERRMSG, TMP.OUTPUT)
  OUTPUTXML := TMP.OUTPUT : '</JobListData>'
RETURN
