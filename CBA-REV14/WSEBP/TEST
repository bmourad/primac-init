 OPEN "","CONTROL" TO CONTROL ELSE
         ERRMSG = "CANNOT OPEN CONTROL FILE"
   RETURN
 END
EMPID="1000"
CONO="001"
PONO ="2336"
FLAG = "Y"
LOGNAME = "CBA"
MANNO = "5"
MANIFESTWGT = "10"
NEWPO =""
**MR=""
**WGT=""
*LOCATION="07"
ERRMSG=""
STRXML = ''
CALL RFT_WRITE.RCV.RF.TRANSFER.1(CONO, EMPID,SERIALID,LOCATION,STRXML,ERRMSG)
CALL RFT_READ.RCV.RF.RECEIVE(CONO, EMPID,PONO,FLAG,LOGNAME,MANNO,MANIFESTWGT,NEWPO,STRXML,ERRMSG)
IF ERRMSG # "" THEN
 WRITE ERRMSG TO CONTROL,"TEST123"
END
RETURN
