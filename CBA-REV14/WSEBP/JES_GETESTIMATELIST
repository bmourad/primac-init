SUBROUTINE JES_GETESTIMATELIST(CONO, SCHEMA.ONLY, ERRMSG, STRXML)
STRXML = '<Estimate><EstimateId>--Select--</EstimateId></Estimate>'
CMD= "SSELECT ESTIMATE WITH @ID LIKE '" : CONO : "...'"
UDTEXECUTE CMD
DATA=1
LOOP
  READNEXT ID ELSE DATA=0
  WHILE DATA DO
    STRXML := '<Estimate><EstimateId>' : ID[4,99] : '</EstimateId></Estimate>'
REPEAT
RETURN
