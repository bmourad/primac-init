SUBROUTINE JES_GETEFF(CONO, JOBID, ERRMSG, OUT_PARAM_EFF,SCHEMA.ONLY)
OUT_PARAM_EFF ='<EFF><DimAround></DimAround><DimAcross></DimAcross><CarWidth></CarWidth><HozSpace></HozSpace><NumLblWide></NumLblWide><LblPerFold></LblPerFold><LblPerCarton></LblPerCarton><FanFoldEvery></FanFoldEvery><NoPerShrk></NoPerShrk></EFF>'
SCHEMA.ONLY = 1
$INCLUDE PMC.CPYLIB COMPANY
$INCLUDE JES.CPYLIB ESTIMATE.RES
*
* OPEN FILES
*
OPEN '','COMPANY' TO COMPANY ELSE ERRMSG = "CANNOT OPEN COMPANY FILE" ;GOTO 99999
OPEN '','ESTIMATE.RES' TO ESTIMATE.RES ELSE ERRMSG = "CANNOT OPEN ESTIMATE.RES FILE" ;GOTO 99999
*
* READ THE COMPANY CODE
*
MATREAD COMP.REC FROM COMPANY,CONO ELSE
      ERRMSG=CONO:"- THIS COMPANY NUMBER DOES NOT EXIST"
      GOTO 99999
END
MATREAD EST.RL.REC FROM ESTIMATE.RES, CONO:JOBID THEN
  SCHEMA.ONLY = 0
  OUT_PARAM_EFF = '<EFF>'
  OUT_PARAM_EFF := '<DimAround>' :EST.RL.DIM.AR : '</DimAround>'
  OUT_PARAM_EFF := '<DimAcross>' :EST.RL.DIM.AC : '</DimAcross>'
  OUT_PARAM_EFF := '<CarWidth>' : EST.RL.CARRIER.WIDTH : '</CarWidth>'
  OUT_PARAM_EFF := '<HozSpace></HozSpace>'
  OUT_PARAM_EFF := '<NumLblWide>' : EST.RL.NO.WIDE : '</NumLblWide>'
  OUT_PARAM_EFF := '<LblPerFold>' : EST.RL.NO.PER.FOLD : '</LblPerFold>'
  OUT_PARAM_EFF := '<LblPerCarton>' : EST.RL.NO.PER.CARTON : '</LblPerCarton>'
  OUT_PARAM_EFF := '<FanFoldEvery>' : EST.RL.FANFOLD.EVERY : '</FanFoldEvery>'
  OUT_PARAM_EFF := '<NoPerShrk></NoPerShrk>'
  OUT_PARAM_EFF := '</EFF>' 
END 
RETURN
*
*
99999*
 IF ERRMSG <> "" THEN
 ERRMSG = "EFF ---> JES_GETEFF---> " : ERRMSG
      CALL WRITELOG(ERRMSG)
 END
 RETURN
 END
