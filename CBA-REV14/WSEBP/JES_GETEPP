SUBROUTINE JES_GETEPP(CONO, JOBID, ERRMSG, OUT_PARAM_EMW,SCHEMA.ONLY)
OUT_PARAM_EMW ='<EPP><LblPerFold></LblPerFold><LblPerCarton></LblPerCarton><LblSheet></LblSheet><CarWidth></CarWidth><DimAround></DimAround><DimAcross></DimAcross><QtyPack></QtyPack><HozSpace></HozSpace><CoreSize></CoreSize><LblPerRoll></LblPerRoll><MaxOD></MaxOD><RollWind></RollWind><RollLblWide></RollLblWide><NumLblWide></NumLblWide><NoPerShrk></NoPerShrk><FanFoldEvery></FanFoldEvery></EPP>'
SCHEMA.ONLY = 1
$INCLUDE PMC.CPYLIB COMPANY
$INCLUDE JES.CPYLIB ESTIMATE.RES
*
* OPEN FILES
*
OPEN '','COMPANY' TO COMPANY ELSE ERRMSG = "CANNOT OPEN COMPANY FILE" ;GOTO 99999
OPEN '','ESTIMATE.RES' TO ESTIMATE.RES ELSE ERRMSG = "CANNOT OPEN ESTIMATE.RES FILE" ;GOTO 99999
*
* READ THE COMPANY CODE
*
MATREAD COMP.REC FROM COMPANY,CONO ELSE
      ERRMSG=CONO:"- THIS COMPANY NUMBER DOES NOT EXIST"
      GOTO 99999
END
MATREAD EST.RL.REC FROM ESTIMATE.RES, CONO:JOBID THEN
  SCHEMA.ONLY = 0
  OUT_PARAM_EMW = '<EPP>'
  OUT_PARAM_EMW := '<LblPerFold>' : EST.RL.NO.PER.FOLD : '</LblPerFold>'
  OUT_PARAM_EMW := '<LblPerCarton>' : EST.RL.NO.PER.CARTON  : '</LblPerCarton>'
  OUT_PARAM_EMW := '<LblSheet></LblSheet>'
  OUT_PARAM_EMW := '<CarWidth>' : EST.RL.CARRIER.WIDTH  : '</CarWidth>'
  OUT_PARAM_EMW := '<DimAround>' : EST.RL.DIM.AR  : '</DimAround>'
  OUT_PARAM_EMW := '<DimAcross>' : EST.RL.DIM.AC : '</DimAcross>'
  OUT_PARAM_EMW := '<QtyPack>': EST.RL.NO.PER.PKG  :'</QtyPack>'
  OUT_PARAM_EMW := '<HozSpace></HozSpace>'
  OUT_PARAM_EMW := '<CoreSize>': EST.RL.CORE.SIZE  :'</CoreSize>'
  OUT_PARAM_EMW := '<LblPerRoll>': EST.RL.NO.PER.ROLL  :'</LblPerRoll>'
  OUT_PARAM_EMW := '<MaxOD>': EST.RL.MAX.OD  :'</MaxOD>'
  OUT_PARAM_EMW := '<RollWind></RollWind>'
  OUT_PARAM_EMW := '<RollLblWide>': EST.RL.LAB.ACR  :'</RollLblWide>'
  OUT_PARAM_EMW := '<NumLblWide>': EST.RL.NO.WIDE   :'</NumLblWide>'
  OUT_PARAM_EMW := '<NoPerShrk></NoPerShrk>'
  OUT_PARAM_EMW := '<FanFoldEvery>' : EST.RL.FANFOLD.EVERY  : '</FanFoldEvery >'
  OUT_PARAM_EMW := '</EPP>' 
END 
RETURN
*
*
99999*
 IF ERRMSG <> "" THEN
 ERRMSG = "EPP ---> JES_GETEPP---> " : ERRMSG
      CALL WRITELOG(ERRMSG)
 END
 RETURN
 END
