COS.REC.SIZE = 30
DIM COS.REC(30)
EQU COS.BUDG.TY        TO COS.REC(1)  ;*MV This Years Budget
EQU COS.BUDG.TYM       TO COS.REC(2)  ;*MV This Year's Modified Budget
EQU COS.BUDG.NY        TO COS.REC(3)  ;*MV Next Year's Budget
EQU COS.BUDG.Y1        TO COS.REC(4)  ;*MV ---------------------------
EQU COS.BUDG.Y2        TO COS.REC(5)  ;*MV              ^
EQU COS.BUDG.Y3        TO COS.REC(6)  ;*MV              |
EQU COS.BUDG.Y4        TO COS.REC(7)  ;*MV Budgets for the Past 6 Years
EQU COS.BUDG.Y5        TO COS.REC(8)  ;*MV              |
EQU COS.BUDG.Y6        TO COS.REC(9)  ;*MV ---------------------------
EQU COS.OPN.BAL.NY     TO COS.REC(10) ;*   Opening Balance - Next Year
EQU COS.PER.BAL.NY     TO COS.REC(11) ;*MV Period Balances - Next Year
EQU COS.OPN.BAL.TY     TO COS.REC(12) ;*   Opening Balance - This Year
EQU COS.PER.BAL.TY     TO COS.REC(13) ;*MV Period Balances - This Year
EQU COS.OPN.BAL.Y1     TO COS.REC(14) ;*   ---------------------------
EQU COS.PER.BAL.Y1     TO COS.REC(15) ;*MV             /^\
EQU COS.OPN.BAL.Y2     TO COS.REC(16) ;*                |
EQU COS.PER.BAL.Y2     TO COS.REC(17) ;*MV              |
EQU COS.OPN.BAL.Y3     TO COS.REC(18) ;*    Opening and Period Balances
EQU COS.PER.BAL.Y3     TO COS.REC(19) ;*MV  for the Past 6 Years
EQU COS.OPN.BAL.Y4     TO COS.REC(20) ;*                |
EQU COS.PER.BAL.Y4     TO COS.REC(21) ;*MV              |
EQU COS.OPN.BAL.Y5     TO COS.REC(22) ;*                |
EQU COS.PER.BAL.Y5     TO COS.REC(23) ;*MV              |
EQU COS.OPN.BAL.Y6     TO COS.REC(24) ;*               \|/
EQU COS.PER.BAL.Y6     TO COS.REC(25) ;*MV  ---------------------------
