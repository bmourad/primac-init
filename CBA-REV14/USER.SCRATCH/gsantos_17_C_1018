$IFNDEF INVRECPWHSE
DIM IRW.REC(20)
$ENDIF
IRW.REC.SIZE = 20
EQU IRW.ORG.QTY TO IRW.REC(1)
EQU IRW.CUR.QTY TO IRW.REC(2)
EQU IRW.RSVB.QTY TO IRW.REC(3)
EQU IRW.UNIT.COST TO IRW.REC(4)
EQU IRW.SERIAL.NO TO IRW.REC(5)
EQU IRW.ON.HAND TO IRW.REC(6)

EQU IRW.JOB TO IRW.REC(7)
EQU IRW.JRSVD.QTY TO IRW.REC(8)
EQU IRW.ORDER TO IRW.REC(7)
EQU IRW.ORSVD.QTY TO IRW.REC(8)
