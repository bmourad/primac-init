$IFNDEF DIVISION
DIM DIV.REC(10)
$ENDIF
DIV.REC.SIZE = 10
EQU DIV.DESC TO DIV.REC(1)
EQU DIV.DEPT TO DIV.REC(2)
EQU DIV.ACTIVE TO DIV.REC(3)
EQU DIV.ADDR1 TO DIV.REC(3)
EQU DIV.ADDR2 TO DIV.REC(4)
EQU DIV.LOCATION TO DIV.REC(4)
EQU DIV.ADDR3 TO DIV.REC(5)
EQU DIV.MSA.USE TO DIV.REC(5)
EQU DIV.ZIP TO DIV.REC(6)
EQU DIV.PRINT TO DIV.REC(6)
EQU DIV.CNTY TO DIV.REC(7)
EQU DIV.LOCATION.CODE TO DIV.REC(7)
EQU DIV.CORP TO DIV.REC(8)
EQU DIV.GEOCODE TO DIV.REC(8)
EQU DIV.LAWSON.NUM TO DIV.REC(8)
EQU DIV.CCTR TO DIV.REC(9)
EQU DIV.OPER TO DIV.REC(10)
