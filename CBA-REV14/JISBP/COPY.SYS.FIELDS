OPEN '','SYS_FIELDS' TO SYS.FIELDS ELSE PRINT "STOPPING" ; STOP
ACTIVE = 1
EXECUTE "GET.LIST RENEE"
LOOP
  READNEXT ID ELSE ACTIVE = 0
WHILE ACTIVE DO
  READ RENEE.REC FROM SYS.FIELDS,ID ELSE RENEE.REC = ""
  IF RENEE.REC NE "" THEN
    NEW.ID1 = FIELD(ID,"_",2)
    NEW.ID2 = FIELD(ID,"_",3)
    NEW.ID = "SHERIDAN_":NEW.ID1
    IF NEW.ID2 NE "" THEN
      NEW.ID = NEW.ID:"_":NEW.ID2
    END
    WRITE RENEE.REC ON SYS.FIELDS,NEW.ID
    DELETE SYS.FIELDS,ID
  END
REPEAT
STOP
