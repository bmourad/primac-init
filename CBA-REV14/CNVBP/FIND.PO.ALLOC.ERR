CRT @(-1):'Looking for PO ALLOC ERRORS'
*COPY>PMC.CPYLIB>PO
*COPY>CPYLIB>CHAR
OPEN 'PO' TO PO ELSE STOP
MORE=1
LOOP
  READNEXT ID ELSE MORE = 0
WHILE MORE DO
  MATREAD PO.REC FROM PO,ID THEN
    FOR I = 1 TO DCOUNT(PO.PROD.NUM,VM)
      IF PO.JB.RECEVED<1,I> # '' THEN
        IF PO.TOT.RECEVED<1,I>+0 # 0 THEN
          IF PO.TOT.RECEVED<1,I> >= SUM(PO.JB.ONORD<1,I>) THEN
            IF SUM(PO.JB.RECEVED<1,I>) < SUM(PO.JB.ONORD<1,I>) THEN
              CRT @(0,5):CL:ID:' TOT.RECV = ':PO.TOT.RECEVED<1,I>'R#12 ':'JOB ORD = ':SUM(PO.JB.ONORD<1,I>)'R#12 ':'JOB RECV = ':SUM(PO.JB.RECEVED<1,I>)'R#12'
              INPUT CLR:
            END
          END
        END
      END
    NEXT I
  END
REPEAT
CRT 'DONE'
