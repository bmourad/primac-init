CRT @(-1)
CRT "Locate missing INV_AUDIT_HIST from INV_SERIAL"
OPEN 'INV_SERIAL' TO INV_SERIAL ELSE STOP
OPEN 'INV_AUDIT_HIST' TO INV_AUDIT_HIST ELSE STOP
OPEN 'EOM.CHECK' TO EOM.CHECK ELSE STOP
DATA=1
LOOP
  READNEXT SID ELSE DATA = 0
WHILE DATA DO
  READ SREC FROM INV_SERIAL,SID THEN
    READ AREC FROM INV_AUDIT_HIST,SID[1,3]:SREC<8,1> ELSE
      WRITE SID ON EOM.CHECK,SID[1,3]:SREC<8,1>
    END
  END
REPEAT
CRT 'DONE'
