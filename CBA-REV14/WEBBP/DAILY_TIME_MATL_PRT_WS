*---- ROUTINE TO PRINT HEADINGS
80000 
   *PRINT CHAR(12)
   *XMLRESULT<-1> = "NEW LINE"
   XMLRESULT<-1> = ""
   PAGE.NO = PAGE.NO + 1
   *PRINT HD1:PAGE.NO
   XMLRESULT<-1> = HD1:PAGE.NO
   *PRINT HD2
   XMLRESULT<-1> = HD2
   *PRINT SPACE(52):ST.DATE:" THROUGH ":ED.DATE:" FOR DIV ":PROC.DIV
   XMLRESULT<-1> = SPACE(52):ST.DATE:" THROUGH ":ED.DATE:" FOR DIV ":PROC.DIV
   LOCATE PAGE.NO-1 IN XMLPAGE<1>,1 SETTING PFIND THEN
       XMLDATA<-1> ="</Page>"
      XMLPAGE = DELETE(XMLPAGE,1,PFIND,0)
   END
   XMLPAGE<1,-1> = PAGE.NO
   XMLDATA<-1> = "<Page PageNo='":PAGE.NO:"'>"
   XMLDATA<-1> = "<StarDate>":ST.DATE:"</StarDate>"
   XMLDATA<-1> = "<EndDate>":ED.DATE:"</EndDate>"
   *PRINT
   *XMLRESULT<-1> = "EMPTY LINE"
   XMLRESULT<-1> = ""
   *PRINT "TRANS DATE : ":OCONV(PREV.TDATE,"D2/"):SPACE(4):"SHIFT : ":PREV.SHIFT
   XMLRESULT<-1> = "TRANS DATE : ":OCONV(PREV.TDATE,"D2/"):SPACE(4):"SHIFT : ":PREV.SHIFT 
   XMLDATA<-1> = "<TransDate>":OCONV(PREV.TDATE,"D2/"):"</TransDate>"
   XMLDATA<-1> = "<Shift>":PREV.SHIFT:"</Shift>"
   *PRINT
   *XMLRESULT<-1> = "EMPTY LINE"
   XMLRESULT<-1> = ""
   *PRINT HH.LINE1
   XMLRESULT<-1> = HH.LINE1
   *PRINT HH.LINE
   XMLRESULT<-1> = HH.LINE
   *PRINT DH.LINE
    XMLRESULT<-1> = DH.LINE
   LINE.COUNT = 9
   RETURN
