XMLDATA<-1> = "<TotalTransDateDetails Date='":OCONV(PREV.TDATE,"D2/"):"'>"
   XMLDATA<-1> = "<JcsUnits Total='":OCONV(TOT.TD<1,5>,"MD2"):"'>"
   T.LINE = T.LINE:"JCS UNITS ":OCONV(TOT.TD<1,5>,"MD2")"R#6":SP1
   T.LINE = T.LINE:OCONV(TOT.TD<1,1>,"MD2")"R#5":SP1
   T.LINE = T.LINE:OCONV(TOT.TD<1,2>,"MD2")"R#5":SP1
   T.LINE = T.LINE:OCONV(TOT.TD<1,3>,"MD2")"R#5":SP1
   T.LINE = T.LINE:OCONV(TOT.TD<1,4>,"MD2")"R#5"
   *PRINT T.LINE
   XMLRESULT<-1> = T.LINE
   XMLDATA<-1> = "<TotalRegularTime>":OCONV(TOT.TD<1,1>,"MD2"):"</TotalRegularTime>"
   XMLDATA<-1> = "<TotalOverTime>":OCONV(TOT.TD<1,2>,"MD2"):"</TotalOverTime>"
   XMLDATA<-1> = "<TotalDT>":OCONV(TOT.TD<1,3>,"MD2"):"</TotalDT>"
   XMLDATA<-1> = "<TotalPrsUnits>":OCONV(TOT.TD<1,4>,"MD2"):"</TotalPrsUnits>"
   XMLDATA<-1> = "</JcsUnits>"
   XMLDATA<-1> = "</TotalTransDateDetails>"
   *PRINT
   *XMLRESULT<-1> = "EMPTY LINE"
   XMLRESULT<-1> = ""
   LINE.COUNT = LINE.COUNT + 1
   PREV.TDATE = TDATE
   FOR XX = 1 TO 5
      GRD.TOT<1,XX> = GRD.TOT<1,XX> + TOT.TD<1,XX>
   NEXT XX
   TOT.TD = ""
