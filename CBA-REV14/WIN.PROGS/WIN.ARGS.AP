SUBROUTINE WIN.ARGS.AP(ARGS)
* Subroutine Parse out arguments from the command line
* Compile for: AP
* (C) Copyright IBM Corporation 2003. All rights reserved
* Copyright (c) 1991-1998. Impact Business Systems
*
COMLIN = OCONV('','U0003')
N = DCOUNT(COMLIN," ")
ARGS = ""
FOR J = 1 TO N
  ARGS<J> = FIELD(COMLIN," ",J)
NEXT J
*
RETURN
*
END
