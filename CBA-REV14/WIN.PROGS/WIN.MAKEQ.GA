      SUBROUTINE WIN.MAKEQ.GA(F.MD, QFILE.DATA, QFILE.MD, ACCOUNTNAME, FILENAME)
* Subroutine to create pointers to remote files
* Compile for: GA
* Copyright (c) 1991-1996. Impact Business Systems
* Copyright (c) 1993-1996. Unidata, Inc.
*
       QREC = 'Q'
       QREC<2> = ACCOUNTNAME
       WRITE QREC ON F.MD, QFILE.MD
       IF FILENAME # "MD" THEN QREC<3> = FILENAME
       WRITE QREC ON F.MD, QFILE.DATA
*
       RETURN
*
END
