SUBROUTINE WIN.OBNEW(NAME,PROGID,RESP)
* Create a new automation object
* Compile for: GENERIC AP MD ME PI.SUB PR.SUB SQ UD UL UP UV UC IN GA
* (C) Copyright IBM Corporation 2003. All rights reserved
* Copyright (c) 1991-99. Impact Business Systems
*
STX=CHAR(2)
SCRIPT = 'Object New ':NAME:',"':PROGID:'",er'
SCRIPT = SCRIPT : ";Enter stx:er"
*
PROMPT ""
ECHO OFF
CALL WIN.COMSUB(SCRIPT)
LOOP
   INPUT DUM:
UNTIL INDEX(DUM,STX,1) DO
REPEAT
*
RESP = FIELD(DUM,STX,2)
*
ECHO ON
RETURN
END
