SUBROUTINE WIN.CHECK.UD(RUNNING)
* Check if wIntegrate is running on UniVerse
* Compile for: UD UC
* (C) Copyright IBM Corporation 2003. All rights reserved.
*
RUNNING = 0
*
CLEARINPUT
PRINT CHAR(13):
CALL WIN.COMSUB("ET stx:1")
BS = @(-9)
PRINT SPACE(10):STR(BS,10):
*
POS = 0
DUM=""
PROMPT ""
ECHO OFF
INPUT DUM: WAITING 1 THEN
   POS = INDEX(DUM,CHAR(2),1)
   IF POS AND DUM[POS+1,1] = "1" THEN RUNNING = 1
END
*
ECHO ON
*
RETURN
*
END
