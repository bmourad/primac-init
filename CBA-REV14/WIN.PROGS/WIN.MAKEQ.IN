      SUBROUTINE WIN.MAKEQ.IN(F.MD, QFILE.DATA, QFILE.MD, ACCOUNTNAME, FILENAME)
* Subroutine to create pointers to remote files
* Compile for: IN
* Copyright (c) 1991-1993. Impact Business Systems
*
       QREC = 'Q'
       QREC<2> = ACCOUNTNAME
       WRITE QREC ON F.MD, QFILE.MD
       IF FILENAME # "MD" THEN QREC<3> = FILENAME
       WRITE QREC ON F.MD, QFILE.DATA
*
       RETURN
*
END
