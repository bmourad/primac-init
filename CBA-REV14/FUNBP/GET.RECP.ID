FUNCTION GET.RECP.ID(CONO,CONTROL,DATE)
CNTL.ID = CONO:"RECP.SEQ"                            
READU RECP.SEQ FROM CONTROL,CNTL.ID ELSE RECP.SEQ = 0
RECP.SEQ+=1                                          
IF RECP.SEQ>9999 THEN RECP.SEQ=1                     
WRITE RECP.SEQ ON CONTROL,CNTL.ID                    
RECP.NO=DATE:RECP.SEQ"R%4"                         
RECP.ID = CONO:RECP.NO                               
RETURN RECP.ID
