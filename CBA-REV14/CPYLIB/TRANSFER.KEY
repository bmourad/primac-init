*********************************
*  TRANSFER KEY FILE DEFINITION *
*     KEY = TRANSFER ID         *
*********************************
      TKEY.REC.SIZE = 10
      DIM TKEY.REC(10)
      EQU TKEY.ACCT          TO TKEY.REC(1)    ;* Account
      EQU TKEY.DICT          TO TKEY.REC(2)    ;* Dictionary
      EQU TKEY.FILE          TO TKEY.REC(3)    ;* File Name
      EQU TKEY.ITEM          TO TKEY.REC(4)    ;* Item Name
      EQU TKEY.OVER          TO TKEY.REC(5)    ;* Overwrite (Y/N)
      EQU TKEY.O.ACCT        TO TKEY.REC(6)    ;* Original Account
      EQU TKEY.TYPE          TO TKEY.REC(7)    ;* Item Type
      EQU TKEY.FUNC          TO TKEY.REC(8)    ;* Item Function
      EQU TKEY.SEQ           TO TKEY.REC(9)    ;* Sequence
