      ORD.DET.REC.SIZE = 50
      DIM ORD.DET.REC(50)
      EQU OSD.STATUS     TO ORD.DET.REC(1)  ;* (MV1) Ship to status
      EQU OSD.STAT.QTY   TO ORD.DET.REC(2)  ;* (MV1)
      EQU OSD.STAT.DATE  TO ORD.DET.REC(3)  ;* (MV1) Status date
      EQU OSD.PO         TO ORD.DET.REC(4)  ;* Ship to PO number
      EQU OSD.PROD       TO ORD.DET.REC(5)  ;* (MV2) Finished goods product
