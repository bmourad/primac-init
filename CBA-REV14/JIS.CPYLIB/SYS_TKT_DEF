************************
*  FILE = SYS_TKT_DEF  *
*  KEY = STKD          *
************************
DIM STKD.REC(30)
EQU STKD_SCN           TO STKD.REC(1) ;* (MV1) Docuset screen Forms
EQU STKD_PRT           TO STKD.REC(2) ;* (MV2) Docuset printer Forms
EQU STKD_PRT_BLN       TO STKD.REC(3) ;* (MV2) Print starting @ line
EQU STKD_PRT_ELN       TO STKD.REC(4) ;* (MV2) Print ending @ line
EQU STKD_LNPERPG       TO STKD.REC(5) ;* Lines per page
EQU STKD_REV_CTL       TO STKD.REC(6) ;* Revision control
EQU STKD_GEN_N_ID      TO STKD.REC(7) ;* Generate sequential ID
EQU STKD_BATCHMODE     TO STKD.REC(8) ;* Batch mode
EQU STKD_TRANSMODE     TO STKD.REC(9) ;* Transfer mode
EQU STKD_SELMODE       TO STKD.REC(10);* Selection mode
EQU STKD_PRT_FORMATSTR TO STKD.REC(11);* (MV2) Format string path (T26868)
EQU STKD_BC_LPTR       TO STKD.REC(13);* Bar Code Printer
EQU STKD_BC_ROW        TO STKD.REC(14);* Bar Code Row
EQU STKD_BC_COL        TO STKD.REC(15);* Bar Code Column
EQU STKD_WITH          TO STKD.REC(16);* (MV3) Selecting WITH criteria 
                                      ;* (A)nd or (O)r.
EQU STKD_WITH_FLD      TO STKD.REC(17);* (MV3) Field Name
EQU STKD_WITH_EQ       TO STKD.REC(18);* (MV3) Matching criteria
EQU STKD_WITH_VAL      TO STKD.REC(19);* (MV3) Matching value
EQU STKD_SORT          TO STKD.REC(20);* (MV4) Sort criteria
EQU STKD_SORT_FLD      TO STKD.REC(21);* (MV4) Field name
EQU STKD_SORT_SUM      TO STKD.REC(22);* (MV4) Summarize records
EQU STKD_TRN_PCFILE    TO STKD.REC(23);* Download to PC file
EQU STKD_PC_APL        TO STKD.REC(24);* Startup PC application
EQU STKD_APL_PCFILE    TO STKD.REC(25);* PC application file option
EQU STKD_SCN_CMP       TO STKD.REC(26);* (MV1) Component dependent scrn
EQU STKD_PRT_CMP       TO STKD.REC(27);* (MV2) Component dependent prt scrn
EQU STKD_BC_FLG        TO STKD.REC(28);* (MV2) Bar Code Print Flag (T22268)
EQU STKD_JOB_FLG       TO STKD.REC(29);* Make ticket/job # the same
                                      ;* Courier (T23224) and Creative
EQU STKD_PG_BRK        TO STKD.REC(29);* Page break flag (Creative)
EQU STKD_DOCUSET_DESC  TO STKD.REC(30);* Lithokrome (T24376)
