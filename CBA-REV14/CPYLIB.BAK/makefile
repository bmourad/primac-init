
#
#  Make file for BP's in CPYLIB
#

BAS = BASIC 
BAS.OPT =
CAT = CATALOG
CAT.OPT = FORCE DIRECT
LOCAL.CAT.OPT = FORCE LOCAL 
BP = CPYLIB
MK = ../SAVEDLISTS/CPYLIB

ALL: _ALT.BUILD.SCREEN _BUILD.SCREEN _CHAR _COLORS _COM.DICT.VAL _COM.DICT.VALUE _COM.FM _COM.GEN.XREF _COM.INSTALL _COM.SCREEN.BUILD _COM1 _COM2 _COM3 _COMM.LINK.STATS _COMMON.FM _COMMON.INSTALL _COMMON.SCREEN.EDIT _COMMON1 _COMMON2 _COMMON3 _COMMON4 _DEMO.FILE _DICT.FILE _DICT.HLD.FILE _DICT.VALUES _DUP.CUSTOMER _EDIT.COM _EDIT.COM.DRIVER _FILE.VARS _GEN.XREF _I.TEST _INSTALL.MAINT _M.SCREENS _MANUAL _MENUS.CHECK _MULTI.LINE.SCRN _NEW.BUILD.SCREEN _NEW.BUILD.Sa _NEW.M.SCREENS _PROG.FILE.XREF _REPORT _SAMPLE _SCOMMON1 _SCREEN.COM _SYSCOM _TABLE.FILE _TAPE.FILE _TRANSFER.KEY _UNI.DICT _USER.MANUAL _USER.TABLE 


