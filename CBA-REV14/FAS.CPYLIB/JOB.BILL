JBIL.REC.SIZE = 10
DIM JBIL.REC(10)
EQU JBIL.CUST         TO JBIL.REC(1) ;* Customer number
EQU JBIL.DATE         TO JBIL.REC(2) ;* Billing Date
EQU JBIL.AMOUNT       TO JBIL.REC(3) ;* Billing Amount
EQU JBIL.COMMENT      TO JBIL.REC(4) ;* Billing Comments
EQU JBIL.JOB.NO       TO JBIL.REC(5) ;* Job number
