* REVISION    - [08.0]
PROMPT ""
      DIM PNUM(1000)
      PROCREAD INBUFF ELSE INBUFF = ""
      CH.NUM = INBUFF<2>
      IF CH.NUM = "" THEN
         PRINT "Enter Number : " : ; INPUT CH.NUM,9
      END
      LOOP
         LOOP
            NOT.OK = 0
            BEGIN CASE
            CASE CH.NUM = ""
               CH.NUM = 0
            CASE NOT(NUM(CH.NUM))
               NOT.OK = 1
            CASE CH.NUM < 0
               NOT.OK = 1
            END CASE
         WHILE NOT.OK DO
            PRINT "Enter Number : " : ; INPUT CH.NUM,9
         REPEAT
      WHILE CH.NUM DO
         PNUM(1) = 1
         LNUM = 1
         GNUM = 1
         CNT = 1
         X = 1
         LOOP
         WHILE CNT < 1000 AND CH.NUM > GNUM DO
            X = X + 1
            BLK = 0
            X2 = INT(X/2)
            FOR I = 2 TO CNT UNTIL BLK
               IF PNUM(I) > X2 THEN GOTO 22
               IF MOD(X,PNUM(I)) = 0 THEN BLK = 1
            NEXT I
22*
            IF BLK = 0 THEN
               CNT = CNT + 1
               PNUM(CNT) = X
               GNUM = X
               IF X < CH.NUM THEN LNUM = X
            END
         REPEAT
         PRINT LNUM, GNUM
         PRINT "Enter Number : " : ; INPUT CH.NUM,9
      REPEAT
   END
