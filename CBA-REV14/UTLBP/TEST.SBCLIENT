* DATE    - 3/12/98
*
* DESCRIPTION
* TEST SBCLIENT BY RUNNING TU.INIT AND TU.FORM.LOAD
*
*********************************************************************
*
   INCLUDE TUBP SPECIAL.H
   INCLUDE TUBP ROC.H
   INCLUDE TUBP OBJECT.H
*
*---- DEFINE VARIABLES
*
   PRINT @(-1)
   PRINT
   PRINT 'TESTING SBCLIENT'
10 *
   FNAME = "PRIMAC.MENUFORMS"
*
*---- OPEN ALL FILES
*
   OPEN FNAME TO PRIMAC.MENUFORMS ELSE PRINT 'CANNOT OPEN PRIMAC.MENUFORMS' ; STOP
*
*---- MAIN PROCESSING
*
110*
   PRINT
   FORMID = "M.MASTER"
   CALL TU.INIT(ERROR)
   BEGIN CASE
      CASE ERROR = 0
         PRINT 'INIT OK'
      CASE 1
         PRINT 'CANNOT INIT, ERROR = ':ERROR:')'
         PRINT 'LOCATE ERROR IN TUBP TU.ERRORCODES.H'
         GOTO 99999
   END CASE
   CALL TU.FORM.LOAD(FNAME,FORMID,"","",FRMHNDL,ERROR)
   BEGIN CASE
      CASE ERROR = 0
         CALL TU.FORM.KILL(FRMHNDL,ERROR)
         PRINT 'LOAD WAS OK'
      CASE 1
         PRINT 'CANNOT LOAD FORM ':FORMID
         PRINT 'LOAD ERROR = (':ERROR:')'
   END CASE
   GOTO 99999
*
*
*---- ERROR PROCESS
*
91000*
   PRINT @(0,23):CL:ERRMSG:
   INPUT REPLY,1:
   PRINT @(0,23):CL:
   RETURN
*
93000*
   PRINT @(0,23):CL:ERRMSG:
   INPUT REPLY,1:
   PRINT @(0,23):CL:
99999*
   STOP
END
