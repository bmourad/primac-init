PRINT "ENTER TYPE (B,P,S,C) :":
INPUT TYPE
PRINT "ENTER LISTNAME :":
INPUT LISTNAME
*
OPEN "VOC" TO VOC ELSE PRINT "NO OPEN VOC" ; STOP
*OPEN "DEVBP" TO DEVBP ELSE PRINT "NO DEVBP" ; STOP
*READ XX FROM DEVBP,"ITEM.LOCATOR.LOCAL" ELSE
*   PRINT "NO ITEM.LOCATOR.LOCAL IN DEVBP IN THIS ACCT."
*   PRINT "THE ORIGINAL IS IN THE PUBP FILE AND NEEDS TO BE"
*   PRINT "COPIED INTO THE DEVBP FILE AND RECOMPILED"
*   STOP
*END
*
VERB = "GET-LIST ":LISTNAME
DONE = 0
CTR = 50
PTR = 0
PROC.HEADER="PQN"
PROC.MAIN = "HITEM.LOCATOR.LOCAL"
PROC.MAIN<-1> = "STON"
PROC.MAIN<-1> = "HY<"
PROC.FOOTER = "HEND<"
PROC.FOOTER<-1> = "HF<"
PROC.FOOTER<-1>="P"
PA.HEADER="PA"
PA.MAIN="RUN ITEM.LOCATOR"
DATA1 = "DATA Y"
UDTEXECUTE VERB
PROC=PROC.HEADER
PA = PA.HEADER
LOOP
  READNEXT ID ELSE DONE=1
UNTIL DONE DO
  IF PTR = 0 THEN
   PROC<-1>=PROC.MAIN
   PA<-1>=PA.MAIN
   PA<-1>=DATA1
  END
  PROC<-1>="H":TYPE:"<"
  PA<-1>="DATA ":TYPE
  PROC<-1>="H":ID:"<"
  PA<-1>="DATA ":ID
  PTR = PTR + 1
  IF PTR > CTR THEN
    PROC<-1>=PROC.FOOTER
    PA<-1>="DATA END"
    PA<-1>="DATA F"
    PTR = 0
  END
REPEAT
IF PTR > 0 THEN
  PROC<-1>=PROC.FOOTER
  PA<-1>="DATA END"
  PA<-1>="DATA F"
END
PRINT "DOING ITEM SEARCH"
WRITE PROC ON VOC, "DO.ITEM.SEARCH"
UDTEXECUTE "DO.ITEM.SEARCH"
STOP
    
  
