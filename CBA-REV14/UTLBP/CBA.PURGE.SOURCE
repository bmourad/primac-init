* REVISION    - [08.0]
DIM FILES(100)
MAT FILES = ''
FCNT = 0
DATA = 1
LOOP
READNEXT FILE.NAME ELSE DATA = 0
WHILE DATA DO
FCNT = FCNT + 1
OPEN '',FILE.NAME TO FILES(FCNT) ELSE
   PRINT 'CANNOT OPEN ':FILE.NAME
   INPUT XX
END
REPEAT
FOR I = 1 TO FCNT
SELECT FILES(I)
DATA = 1
LOOP
READNEXT ID ELSE DATA = 0
WHILE DATA DO
IF ID[1,1] # '$' THEN DELETE FILES(I), ID
REPEAT
NEXT I
END
