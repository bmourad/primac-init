*
100 *
STMNT = "SELECT ":FNAME
UDTEXECUTE STMNT
REM SELECT FNAME
110 *
 READNEXT ID ELSE GOTO 199
IF ID[1,1] NE '_' THEN GOTO 110
CRT "DELETE ":ID:" ":FNAME:" ! "
DELETE PFILE, ID
GOTO 110
199 * RETURN
RETURN
*
GOTO 245
200 * CLEAN UP COMPILE.MSGS *
CRT "CLEAN UP COMPILE.MSGS"
OPEN 'COMPILE.MSGS' TO COMPILE.MSGS ELSE STOP
SELECT COMPILE.MSGS
210 READNEXT ID ELSE GOTO 245
CNT = FIELD(ID,"!",1)
IF NOT(NUM(CNT)) THEN GOTO 210
IF CNT > 6 THEN DELETE COMPILE.MSGS, ID
GOTO  210
*
*
*
245 *
CRT "DONE WITH CLEANUP"
