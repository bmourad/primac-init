OPEN '','VOC' TO VOC ELSE
   ERRMSG='CANNOT OPEN VOC FILE'
   GOSUB 999
   STOP
END
PROMPT1='INPUT DEMO PATH e.g. /usr/ud33/primac/CBA-REV10A: '
PROMPT2='INPUT TRAIN PATH e.g. /cust/train/TRAIN: '
PROMPT3='INPUT PROD PATH e.g. /cust/prod/CUST: '
PRINT @(0) 
PRINT @(0,5):PROMPT1:
INPUT DEMOPATH
PRINT @(0,7):"ECHO DEMO PATH":DEMOPATH:
GOTO 9999
999:
PRINT @(0,23):@(-4):ERRMSG:
INPUT JUNK
PRINT @(0,23):@(-4):
RETURN
9999:
STOP
