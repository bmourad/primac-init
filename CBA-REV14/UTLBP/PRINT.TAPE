10 *
PRINT CHAR(12)
PRINT @(0,23):"ENTER RECORD SIZE ":
INPUT REC.SIZE
IF REC.SIZE = "END" THEN GOTO 99
IF NOT(NUM(REC.SIZE)) OR REC.SIZE = 0 THEN GOTO 10
PRINTER ON
PRINT CHAR(12)
100*
READT X ELSE
  PRINTER CLOSE
  GOTO 10
END
XX = ASCII(X)
*XX = X
NUM = INT(LEN(XX)/REC.SIZE)
PTR = 1
FOR I = 1 TO NUM
PRINT XX[PTR,REC.SIZE]
PTR = PTR + REC.SIZE
NEXT I
GOTO 100
99*
END
