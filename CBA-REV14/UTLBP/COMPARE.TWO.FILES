OPEN "","COMPARE.FILES" TO CFILE ELSE STOP
PROCREAD BUFFER ELSE STOP
FROMFILE = BUFFER<1>
TOFILE = BUFFER<2>
OPEN "",FROMFILE TO FFILE ELSE STOP
OPEN "",TOFILE TO TFILE ELSE STOP
CLEARFILE CFILE
DATA = 1
LOOP
   READNEXT ID ELSE DATA = 0
WHILE DATA DO
*  PRINT ID
   READ FREC FROM FFILE, ID THEN
      READ TREC FROM TFILE, ID THEN
         IF FREC # TREC THEN
            WRITE "MISSMATCH" ON CFILE, ID
         END
      END ELSE
         WRITE "" ON CFILE, ID
      END
   END
REPEAT
END
