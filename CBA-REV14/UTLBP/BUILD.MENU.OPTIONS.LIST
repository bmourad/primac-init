OPEN "PRIMAC.MENUS" TO PRIMAC.MENUS ELSE PRINT "NO PRIMAC.MENUS"
OPEN "MENU.OPTIONS" TO MENU.OPTIONS ELSE PRINT "NO MENU.OPTIONS"
DONE=0
SELECT PRIMAC.MENUS
LOOP
READNEXT ID ELSE DONE = 1
UNTIL DONE DO
READ REC FROM PRIMAC.MENUS,ID THEN
NUM.ATTR=DCOUNT(REC,@AM)
FOR II = 1 TO NUM.ATTR
  CID = REC<II,1>
 IF CID = "C" THEN
   * COMMENT SKIP
 END ELSE
    IF CID[1,2] = "M." THEN
      * MENUS SKIP
    END ELSE
      WRITE "" ON MENU.OPTIONS,CID
    END
 END
NEXT II
END
REPEAT
