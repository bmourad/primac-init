* REVISION    - [08.0]
EQU AM TO CHAR(254)
PRINTER ON
HEADING "PRIME NUMBERS FROM 1 TO 1000"
PNUM = 1
CNT = 1
PRINT PNUM
FOR X = 2 TO 1000
   BLK = 0
   FOR I = 2 TO CNT UNTIL BLK
      IF MOD(X,PNUM<I>) = 0 THEN BLK = 1
   NEXT I
   IF BLK = 0 THEN
      PRINT X
      PNUM<-1> = X
      CNT = CNT + 1
   END
NEXT X
PRINTER OFF
STOP
END
