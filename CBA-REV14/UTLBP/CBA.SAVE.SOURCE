* REVISION    - [08.0]
     OPEN '','MD' TO MD ELSE ERRMSG = 'MD FILE IS MISSING'; GOTO 93000
     CL = "K"
     DATA = 1
     ACCOUNT = 'CBA'
     LOGTO.ACCOUNT = 'SAVE-CBA'
     BEGIN.STMT = 'HCREATE-FILE ('
     END.STMT = ' 1,1 97,1)'
     CLEAR.STMT = 'HCLEAR-FILE (DATA '
     COPY.STMT = 'HCOPY QFILE *'
     TO.STMT = 'H('
     PROC = ''
     LN = 1
     PROC<LN> = 'PQN'
     LN = LN + 1; PROC<LN> = 'HLOGTO ' : LOGTO.ACCOUNT
     LN = LN + 1; PROC<LN> = 'P'
     LOOP
        READNEXT ID ELSE DATA = 0
     WHILE DATA DO
        LN = LN + 1; PROC<LN> = BEGIN.STMT : ID : END.STMT
        LN = LN + 1; PROC<LN> = 'P'
        LN = LN + 1; PROC<LN> = 'T C'
        LN = LN + 1; PROC<LN> = 'F-C 1'
        LN = LN + 1; PROC<LN> = 'F-OPEN 1 MD'
        LN = LN + 1; PROC<LN> = 'XCAN NOT OPEN MD'
        LN = LN + 1; PROC<LN> = 'F-READ 1 QFILE'
        LN = LN + 1; PROC<LN> = 'XCAN NOT READ QFILE'
        LN = LN + 1; PROC<LN> = 'MV &1.2 "':ACCOUNT:'"'
        LN = LN + 1; PROC<LN> = 'MV &1.3 "' : ID : '"'
        LN = LN + 1; PROC<LN> = 'F-WRITE 1'
        LN = LN + 1; PROC<LN> = CLEAR.STMT : ID : ')'
        LN = LN + 1; PROC<LN> = 'P'
        LN = LN + 1; PROC<LN> = COPY.STMT
        LN = LN + 1; PROC<LN> = 'STON'
        LN = LN + 1; PROC<LN> = TO.STMT : ID : ')'
        LN = LN + 1; PROC<LN> = 'P'
     REPEAT
     LN = LN + 1; PROC<LN> = 'HLOGTO ':ACCOUNT
     LN = LN + 1; PROC<LN> = 'P'
     WRITE PROC ON MD, "CBA.SAVE.SOURCE"
     GOTO 99999
93000 PRINT @(0,23) : CL : ERRMSG :; INPUT XX :
99999 END
