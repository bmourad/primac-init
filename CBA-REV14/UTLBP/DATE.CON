* REVISION    - [08.0]
10 *
      CRT "INPUT INTERNAL OR EXTERNAL DATE":;INPUT DT
      IF DT = "" THEN STOP
      IF NUM(DT) THEN
         CRT OCONV(DT,"D2/")
         CRT OCONV(DT,"D")
      END ELSE
         CRT ICONV(DT ,"D")
         CRT OCONV(ICONV(DT,"D"),"D")
      END
      GOTO 10
      END
