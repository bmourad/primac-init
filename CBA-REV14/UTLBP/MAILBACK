   VMAIL.BODY = ""
   MAIL.HEADER = ""
   MAIL.HEADER<1,1> = "Email Request Contact Listing"
   MAIL.HEADER<1,2> = "Request Date     : ":OCONV(DATE(), "D4/")
   DPATH = "./_HOLD_/jihadccdllhead"
   MAIL.KEY = DPATH
   MAIL.MSG = MAIL.HEADER
   SWAP CHAR(253) WITH CHAR(10) IN MAIL.MSG
   OSWRITE MAIL.MSG ON MAIL.KEY
   DPATH = "./_HOLD_/jihadccdll.rtf"
   MAIL.KEY1 = DPATH
   SUBJECT = "Email Request Contact listing"
   WW.USER.EMAIL = "jyamout@vercom.com"
   VOCKEY = "jihadccdll"
   IF WW.USER.EMAIL # "" THEN
      MAIL.COMM = "!/usr/local/bin/mpack -s "
      MAIL.COMM = MAIL.COMM:QUOTE(SUBJECT):" -d "
      MAIL.COMM = MAIL.COMM:QUOTE(MAIL.KEY):" "
      MAIL.COMM = MAIL.COMM:QUOTE(MAIL.KEY1):" "
      MAIL.COMM = MAIL.COMM:WW.USER.EMAIL
      UPDATE.VOC.FLAG = 1
      OPEN "","VOC" TO VOC ELSE UPDATE.VOC.FLAG = 0
      IF UPDATE.VOC.FLAG = 1 THEN
         REC = ""
         REC<1> = "PQN"
         REC<2> = "H ":MAIL.COMM
         REC<3> = "P"
         WRITE REC ON VOC , VOCKEY
      END
      EXECUTE "PHANTOM ":VOCKEY
   END
   RETURN
