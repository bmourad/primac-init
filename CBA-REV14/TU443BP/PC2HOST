 INCLUDE TUBP USER.INCLUDE.H
 INCLUDE TUBP ROC.H
*
 INCLUDE TUBP SPECIAL.H
*
*
ERROR = FALSE
*
 INCLUDE TUBP CASE.INVERT.OFF.H
*
CALL TU.INIT(ERROR)
IF ERROR THEN CRT 'ERROR! Cannot Init'; GO 999
PARENT = NUL
CALL ROC.MAIN(ROC.GET.ATTRIB, "this", "parent", PARENT, ERROR)
HSESS = PARENT
CALL ROC.MAIN(ROC.GET.ATTRIB, PARENT, "mdi_client_form", PARENT, ERROR)
IF NOT(ERROR) THEN
ATTS = "rpc_startup"
VALS = "pc2hostclass"
NAME = "PC2HOST":HSESS
VALS<2> = "parent":VM:"session_handle":VM:"run":VM:"object_name"
VALS<3> = PARENT :VM:HSESS :VM:TRUE :VM:NAME
CALL ROC.MAIN(ROC.SERVE, "this", ATTS, VALS, ERROR)
IF NOT(ERROR) THEN
VALS = ''
CALL ROC.MAIN(ROC.GET.HANDLE, NAME, '', VALS, ERROR)
IF VALS # NUL THEN CALL ROC.MAIN(ROC.DESTROY, VALS, NUL, NUL, ERROR)
END
END
*CALL TU.TERMINATE(ERROR)
999 *
IF PTERM THEN EXECUTE "PTERM CASE INVERT"
END
