OPEN "","INV.WHSE" TO INV.WHSE ELSE STOP
100 READNEXT ID ELSE STOP
   READU ITEM FROM INV.WHSE, ID ELSE
    RELEASE INV.WHSE, ID
    GOTO 100
   END
   ITEM<2>=0
   ITEM<3>=0
   ITEM<4>=0
   ITEM<7>=0
   ITEM<8>=0
   ITEM<9>=0
   ITEM<10>=0
   FOR XX = 14 TO 48
    ITEM<XX>=""
   NEXT XX
   ITEM<51>=0
   WRITE ITEM ON INV.WHSE,ID
  GOTO 100
END
