CRT @(-1)
OPEN '','CONTROL' TO CONTROL  ELSE STOP 201 
IF RECORDLOCKED(CONTROL,"001ISTK.SEQ")=0 THEN
  PRINT "IT IS NOT LOCKED"
END ELSE
PRINT "IT IS LOCKED"
END
READU CHECK FROM CONTROL,"001ISTK.SEQ" ELSE CHECK=0
IF RECORDLOCKED(CONTROL,"001ISTK.SEQ")=0 THEN
  PRINT "IT IS NOT LOCKED"
END ELSE
PRINT "IT IS LOCKED"
END
INPUT QQ
