SUBROUTINE PCHECK
* REVISION    - [08.0]
* Copyright 1982 by Computer Business Associates (Vercom Software, Inc.)
*
*
*
*
*
OPEN'',"CONTROL" TO CONTROL ELSE STOP
 OPEN "","MD" TO MD ELSE STOP
SP=OCONV(0,"U10DD")
READ NUM FROM CONTROL,"001CONTROLDATES" ELSE DELETE MD,"PR";DELETE MD,"PR.P";STOP
   FOR X = 1 TO X + 1
   IF NUM<1,X,0> = '' THEN DELETE MD,"PR";DELETE MD,"PR.P";STOP
   IF NUM<1,X,0> = SP THEN RETURN ELSE GOTO 10
   10 NEXT X
END
