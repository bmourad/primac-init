REM "DELETES 'DELETE' RECORDS FROM PAYREG (PAYROLL) PAYROLL PAYREGDELETE"
**********************************
     CLEAR
     OPEN "","PAYREG" TO PAYREG ELSE PRINT "CAN'T OPEN PAYREG";STOP
     OPEN "","DEDREG" TO DEDREG ELSE PRINT "CAN'T OPEN DEDREG";STOP
100  READNEXT ID ELSE STOP
      DELETE PAYREG,ID;DELETE DEDREG,ID
     GOTO 100
END
