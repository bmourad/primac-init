PRINT "ENTER FILENAME :":
INPUT FILENAME
OPEN FILENAME TO FNAME THEN
  AM = CHAR(254)
  DONE=0
  PGNUM=1
  LOOP
    READNEXT ID ELSE DONE=1
  UNTIL DONE DO
    READ REC FROM FNAME,ID THEN
      CTR=0
      CTR=99
      NUM.LINES = DCOUNT(REC,AM)
      PRINTER ON
      FOR II = 1 TO NUM.LINES
        IF CTR > 55 THEN 
           PRINT CHAR(12):"Page ":PGNUM"R%4 ":FILENAME:"   ":ID
           PRINT "Printed for ":@LOGNAME
           PGNUM=PGNUM+1
           PRINT ; PRINT
           CTR=0
        END
        PRINT ((II"R%5")"L#7"):REC<II>
        CTR=CTR+1
      NEXT II
      PRINTER OFF
    END
  REPEAT
END
