PRINT "ENTER TYPE (B,P,S,C) :":
INPUT TYPE
PRINT "ENTER LISTNAME :":
INPUT LISTNAME
*
OPEN "VOC" TO VOC ELSE PRINT "NO OPEN VOC" ; STOP
*
VERB = "GET-LIST ":LISTNAME
DONE = 0
CTR = 50
PTR = 0
PROC.HEADER="PQN"
PROC.MAIN<-1> = "HITEM.LOCATOR"
PROC.MAIN<-1> = "STON"
PROC.FOOTER = "H":CHAR(27):"<"
PROC.FOOTER<-1>="P"
UDTEXECUTE VERB
PROC=PROC.HEADER''
LOOP
  READNEXT ID ELSE DONE=1
UNTIL DONE DO
  IF PTR = 0 THEN
   PROC=PROC:PROC.MAIN
  END
  PROC=PROC:TYPE:"<"
  PROC=PROC:ID:"<"  
  PTR = PTR + 1
  IF PTR > CTR THEN
    PROC<-1>=PROC.FOOTER
    PTR = 0
  END
REPEAT
IF PTR > 0 THEN
  PROC<-1>=PROC.FOOTER
    
  
