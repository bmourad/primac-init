*****************************
* FREIGHT ON BOARD - FOB FILE DEFINITION
* FOB.KEY = CONO:FOB
*********************************
FOB.REC.SIZE = 15
DIM FOB.REC(15)
EQU FOB.DESC      TO FOB.REC(1)
EQU FOB.SURCHARGE TO FOB.REC(2)
EQU FOB.DISCOUNT  TO FOB.REC(3)
EQU FOB.RATE      TO FOB.REC(4) ;* (MV1)
EQU FOB.WEIGHT    TO FOB.REC(5) ;* (MV1)
EQU FOB.MIN.LBS   TO FOB.REC(6) 
EQU FOB.MIN.CHG   TO FOB.REC(7)
