GLP.REC.SIZE = 5
DIM GLP.REC(5)
EQU GLP.NO.PRDS    TO GLP.REC(1)    ; * Number of Periods in a Year
EQU GLP.NO.PERIODS TO GLP.REC(1)
EQU GLP.START      TO GLP.REC(2)    ; * (MV) Start Dates for Periods
EQU GLP.OPEN.DATES TO GLP.REC(3)
