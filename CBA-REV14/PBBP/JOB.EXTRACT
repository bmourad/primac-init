SUBROUTINE JOB.EXTRACT(RTN,CONO,JOBNO)
*
*COPY>JCS.CPYLIB>JOB
OPEN '','JOB' TO JOB ELSE STOP
OPEN '','JOB.SCHED' TO JOB.SCHED ELSE STOP
PARAM = ''
PARAM<1,1> = CONO
PARAM<1,2> = JOBNO
PARAM<1,3> = 'N'
*
MATREAD JOB.REC FROM JOB, CONO:JOBNO ELSE RETURN
IF JOB.STATUS<1,1> > 2 THEN RETURN
READ REC FROM JOB.SCHED, CONO:JOBNO THEN
  PARAM<1,4> = 'R'
END ELSE
  IF JOB.EST # "" THEN
    PARAM<1,4> = 'P'
  END ELSE
    RETURN
  END
END
CALL PERFORM.PROG('JOB.SCHED.EXTRACT', PARAM)
*
RETURN
