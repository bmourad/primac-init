SUBROUTINE TESTZY(AGE_BAL,AGE_TYPE,AGE_DAYS,INV_DATE,INV_BAL)
*
      BEGIN CASE
      CASE INV_BAL > 0
         DAYS_DUE = DATE() - INV_DATE
         IF AGE_TYPE = "OVER" THEN
            IF DAYS_DUE > AGE_DAYS THEN
               AGE_BAL = INV_BAL
            END ELSE
               AGE_BAL = ""
            END
         END ELSE
            IF DAYS_DUE < AGE_DAYS THEN
               AGE_BAL = INV_BAL
            END ELSE
               AGE_BAL = ""
            END
         END
      CASE INV_BAL < 0
         IF AGE_TYPE = "CR" THEN
            AGE_BAL = INV_BAL
         END ELSE
            AGE_BAL = ""
         END
      CASE 1
         AGE_BAL = ""
      END CASE
RETURN
END
