        ESTJA.OS.DCOST = INSERT(ESTJA.OS.DCOST,1,D,0,"")
        ESTJA.OS.COST = INSERT(ESTJA.OS.COST,1,D,0,"0")
        ESTJA.OS.SALE = INSERT(ESTJA.OS.SALE,1,D,0,"0")
