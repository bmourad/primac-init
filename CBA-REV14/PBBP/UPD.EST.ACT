        LOCATE JMT.DEPT IN ESTJA.MT.DEPT<1>,PTR BY "AR" SETTING D THEN
          BEGIN CASE
          CASE JMT.CCTR = ESTJA.MT.CCTR<1,D>
            BEGIN CASE
            CASE JMT.PROD = ESTJA.MT.PROD<1,D>
              PTR = 0
