
#
#  Make file for BP's in MACRO
#

BAS = BASIC 
BAS.OPT =
CAT = CATALOG
CAT.OPT = FORCE DIRECT
LOCAL.CAT.OPT = FORCE LOCAL 
BP = MACRO
MK = ../SAVEDLISTS/MACRO

ALL: _CHECK.ORDER.COMPLETE _CHECK.ORDERa _CLEAR.SCREEN _COUNT.SCREEN _DEFINE.SCREEN _DEFINE.SCREa _DELVAL _DISPLAY.SCREEN _DISPLAY.SCRa _FIELD.SCREEN _FORMAT.SCREEN _FORMAT.SCREa _INIT.SCREEN _INPUT.SCREEN _INSVAL _LOG.COUNT _LOG.INIT _LOG.INIT.SUB _LOG.TERM _LOG.WRITE _LOG.WRITE.SUB _LOG.WRITE.Sa _MULTI.SCREEN _PAYMENT.CALC _REPVAL _V.DISPLAY.SCREEN _V.DISPLAY.Sa _V.MULTI.SCREEN _V.MULTI.SCRa 


_CHECK.ORDER.COMPLETE: CHECK.ORDER.COMPLETE 
	echo "$(BAS) $(BP) 'CHECK.ORDER.COMPLETE' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'CHECK.ORDER.COMPLETE' $(CAT.OPT) " >> $(MK)

_CHECK.ORDERa: CHECK.ORDERa 
	echo "$(BAS) $(BP) 'CHECK.ORDERa' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'CHECK.ORDERa' $(CAT.OPT) " >> $(MK)

_CLEAR.SCREEN: CLEAR.SCREEN 
	echo "$(BAS) $(BP) 'CLEAR.SCREEN' $(BAS.OPT)" >> $(MK)
	echo "$(CAT) $(BP) 'CLEAR.SCREEN' $(CAT.OPT) " >> $(MK)

