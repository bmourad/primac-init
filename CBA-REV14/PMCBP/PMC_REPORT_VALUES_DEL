   SUBROUTINE PMC_REPORT_VALUES_DEL (RPT_SUBID, PMC_REPORT_VALUES, ERRMSG)
*COPY>CPYLIB>CHAR
   IF FILEINFO (PMC_REPORT_VALUES, 0) # 1 THEN
     OPEN "","PMC_REPORT_VALUES" TO PMC_REPORT_VALUES ELSE
       ERRMSG = "PMC_REPORT_VALUES file is missing"
       GOTO 99999
     END
   END
   RPT_COUNT = DCOUNT(RPT_SUBID<1>,VM)
   FOR II = 1 TO RPT_COUNT
     READU DUMMY FROM PMC_REPORT_VALUES, RPT_SUBID<1,II> THEN
       DELETE PMC_REPORT_VALUES, RPT_SUBID<1,II>
     END ELSE
       RELEASE PMC_REPORT_VALUES, RPT_SUBID<1,II>
     END
   NEXT II
   RPT_SUBID = ""
99999*
   RETURN
 END
