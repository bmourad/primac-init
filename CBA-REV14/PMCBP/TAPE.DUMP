   OPEN 'CONTROL' TO CONTROL ELSE STOP
   EQU AM TO CHAR(254)
   IN.PROC = 1
   PROCREAD BUFFER ELSE IN.PROC = 0
   READV UNIT FROM CONTROL, BUFFER<1>:'JOB.PURGE.TAPE', 1 ELSE
      BUFFER<3> = 'END'
      GOTO 10
   END
   BUFFER<3> = UNIT
   FILE = BUFFER<7>
   UDTEXECUTE "T.DUMP ":FILE:" MU ":UNIT CAPTURING ERRMSG
   FINDSTR "dumped" IN ERRMSG,1 SETTING FLG THEN
      BUFFER<7> = "OK"
   END
   P_X  = 0 ; P_Y = 23 ; P_VALUE = BUFFER<7> ; P_OPT = ""
   P_X  := AM:0 ; P_Y := AM:23 ; P_VALUE := AM:ERRMSG
   CALL VSI_PLINE(P_X,P_Y,P_VALUE,P_OPT)
10 *
   PROCWRITE BUFFER
