     FOR L = 1 TO COA.REC.SIZE ; COA.REC(L) = ECD.VALDAT.ITEM<L> ; NEXT L
     SCV.REC(ECD.NUM)<ECD.SCRN.NO> = GLTB.PR.EARN CO.ACCT.MASK ;ECD.ACTION = 5;CALL SCRN.EDIT
     ECD.NUM = 4 ;SCV.REC(ECD.NUM)<ECD.SCRN.NO> = COA.DESC ;ECD.ACTION = 5;CALL SCRN.EDIT
  END
 RETURN
**** ENTER PAYROLL FICA
1020*
  ECD.NUM = 5 ; SCV.REC(ECD.NUM)<ECD.SCRN.NO> = "" ;ECD.ACTION = 5 ; CALL SCRN.EDIT
  ECD.MAXL = IN.ACCT.LEN ;ECD.VALDAT.CODE = 2;ECD.VALDAT.FILE = COA;ECD.PREFIX.ID = CONO
  SCV.REC(ECD.NUM)<ECD.SCRN.NO> = GLTB.FICA ; ECD.PATRN = CO.ACCT.MATCH ;ECD.ACTION = 4;CALL SCRN.EDIT
  IF ECD.RET.VALUE # "END" THEN
     GLTB.FICA = ECD.RET.VALUE
     FOR L = 1 TO COA.REC.SIZE ; COA.REC(L) = ECD.VALDAT.ITEM<L> ; NEXT L
     SCV.REC(ECD.NUM)<ECD.SCRN.NO> = GLTB.FICA CO.ACCT.MASK ;ECD.ACTION = 5;CALL SCRN.EDIT
     ECD.NUM = 6 ;SCV.REC(ECD.NUM)<ECD.SCRN.NO> = COA.DESC ;ECD.ACTION = 5;CALL SCRN.EDIT
  END
 RETURN
**** ENTER PAYROLL EIC
1030*
  ECD.NUM = 7 ; SCV.REC(ECD.NUM)<ECD.SCRN.NO> = "" ;ECD.ACTION = 5 ; CALL SCRN.EDIT
  ECD.MAXL = IN.ACCT.LEN ;ECD.VALDAT.CODE = 2;ECD.VALDAT.FILE = COA;ECD.PREFIX.ID = CONO
  SCV.REC(ECD.NUM)<ECD.SCRN.NO> = GLTB.PR.EIC ; ECD.PATRN = CO.ACCT.MATCH ;ECD.ACTION = 4;CALL SCRN.EDIT
  IF ECD.RET.VALUE # "END" THEN
     GLTB.PR.EIC = ECD.RET.VALUE
     FOR L = 1 TO COA.REC.SIZE ; COA.REC(L) = ECD.VALDAT.ITEM<L> ; NEXT L
     SCV.REC(ECD.NUM)<ECD.SCRN.NO> = GLTB.PR.EIC CO.ACCT.MASK ;ECD.ACTION = 5;CALL SCRN.EDIT
     ECD.NUM = 8 ;SCV.REC(ECD.NUM)<ECD.SCRN.NO> = COA.DESC ;ECD.ACTION = 5;CALL SCRN.EDIT
  END
 RETURN
**** ENTER FEDERAL INCOME TAX ACCOUNT
1040*
  ECD.NUM = 9 ; SCV.REC(ECD.NUM)<ECD.SCRN.NO> = "" ;ECD.ACTION = 5 ; CALL SCRN.EDIT
  ECD.MAXL = IN.ACCT.LEN ;ECD.VALDAT.CODE = 2;ECD.VALDAT.FILE = COA;ECD.PREFIX.ID = CONO
  SCV.REC(ECD.NUM)<ECD.SCRN.NO> = GLTB.PR.FIT ; ECD.PATRN = CO.ACCT.MATCH ;ECD.ACTION = 4;CALL SCRN.EDIT
  IF ECD.RET.VALUE # "END" THEN
     GLTB.PR.FIT = ECD.RET.VALUE
     FOR L = 1 TO COA.REC.SIZE ; COA.REC(L) = ECD.VALDAT.ITEM<L> ; NEXT L
     SCV.REC(ECD.NUM)<ECD.SCRN.NO> = GLTB.PR.FIT CO.ACCT.MASK ;ECD.ACTION = 5;CALL SCRN.EDIT
     ECD.NUM = 10 ;SCV.REC(ECD.NUM)<ECD.SCRN.NO> = COA.DESC ;ECD.ACTION = 5;CALL SCRN.EDIT
  END
 RETURN
**** ENTER SAVINGS BOND ACCOUNT
1050*
  ECD.NUM = 11 ; SCV.REC(ECD.NUM)<ECD.SCRN.NO> = "" ;ECD.ACTION = 5 ; CALL SCRN.EDIT
  ECD.MAXL = IN.ACCT.LEN ;ECD.VALDAT.CODE = 2;ECD.VALDAT.FILE = COA;ECD.PREFIX.ID = CONO
  SCV.REC(ECD.NUM)<ECD.SCRN.NO> = GLTB.PR.SAV.BOND ; ECD.PATRN = CO.ACCT.MATCH ;ECD.ACTION = 4;CALL SCRN.EDIT
  IF ECD.RET.VALUE # "END" THEN
     GLTB.PR.SAV.BOND = ECD.RET.VALUE
     FOR L = 1 TO COA.REC.SIZE ; COA.REC(L) = ECD.VALDAT.ITEM<L> ; NEXT L
     SCV.REC(ECD.NUM)<ECD.SCRN.NO> = GLTB.PR.SAV.BOND CO.ACCT.MASK ;ECD.ACTION = 5;CALL SCRN.EDIT
     ECD.NUM = 12 ;SCV.REC(ECD.NUM)<ECD.SCRN.NO> = COA.DESC ;ECD.ACTION = 5;CALL SCRN.EDIT
  END
 RETURN
***** CALLS FOR SYSCOM
91000 ERR.TYPE = 1
      CALL SYSCOM(MAT SYSCOM.REC)
      RETURN
92000 ERR.TYPE = 2
      CALL SYSCOM(MAT SYSCOM.REC)
      RETURN
93000 ERR.TYPE = 3
      CALL SYSCOM(MAT SYSCOM.REC)
99999 *
     ECD.ACTION=99;CALL SCRN.EDIT
     RETURN
      END
