     IN.PROC = 1
     PROCREAD BUFFER ELSE IN.PROC = 0
     IF IN.PROC THEN
        I = 1
        LOOP WHILE BUFFER<I> # '' DO
           I = I + 1
        REPEAT
        BUFFER<I> = "END"
        PROCWRITE BUFFER
        REWIND ELSE GOTO 10
        BUFFER<I> = "OK"
        PROCWRITE BUFFER
10*
     END
     END
