   SUBROUTINE CONTROL.SETUP.LIST(CONO)
*COPY>CPYLIB>COM1
*********************************************************************
* REVISION    - [08.1]
* Copyright 1982 by Computer Business Associates (Vercom Software, Inc.)
* SYSTEM      - PRIMAC
* SOURCE      - PMCBP
* PROGRAM     - CONTROL.SETUP.LIST
* BY          - WALID YAMOUT, COMPUTER BUSINESS ASSOCIATES
* DATE        - 11/02/84
* MODIFIED    - 08/23/88 BY KERRY WILSON
* DESCRIPTION -
*ENDOC
*T21893 gat 05/07/1997 * FIX JOB NUMBER NOT PRINTING
*T21643 larryt 05/15/1997 * PRINT # OF PERIODS IN GENERAL LEDGER SECTION
*T24541 gil  11/03/99  * FIX DIE FIELD ON REPORT.
*T26227 edwin 10/15/2001 * Control setup listing re-formatted for
*                          divisional security.
*T26556 cmykleb 06/17/2002 * Rev12 changes.
*T28418 thompson 01/25/2005 * Fix page numbers on report
*********************************************************************
*COPY>CPYLIB>FILE.VARS
*COPY>CPYLIB>CHAR
* INITIALIZATION
   DASHES = STR('-',80)
   SP2 = STR(" ",2)
   UNKNOWN = '????????????????????'
   NSET = "NEED SETUP"
   PAGE.NO = 0
   LINE.CNT = 0
* MAIN PROCESSING
   READ REC FROM CONTROL, "MENUS.CONTROL" ELSE
      REC = ""
      REC<3> = NSET
   END
   MENUS.CONTROL = "FOR ACCOUNT - " : REC<3>
*T26556 v
   RPT.NO = "PC125"
   CONO.NAME = ""
   RPT.NAME = ""
   HEAD1 = ""
   HEAD2 = ""
   CALL GET.PROG.HEAD(CONO,CONO.NAME,RPT.NAME,RPT.NO,"",HEAD1,HEAD2)
*T26556 ^
   PRINTER ON
   GOSUB 1000 
*--- ARS
   PRINT "      ACCOUNT RECEIVABLE"
   PRINT DASHES[1,30]
   PRINT
   READ REC FROM CONTROL, CONO:"PPINV#" ELSE REC = NSET
   PRINT SP2:"Prepayment Number    - ":REC
   READ REC FROM CONTROL, CONO:"BCINV#" ELSE REC = NSET
   PRINT SP2:"Bad Checks Number    - ":REC
   READ REC FROM CONTROL, CONO:"ACINV#" ELSE REC = NSET
   PRINT SP2:"On Account Number    - ":REC
   READ REC FROM CONTROL, CONO:"CRREF" ELSE REC = NSET
   PRINT SP2:"Cash Rec. Reference  - ":REC
   LINE.CNT = LINE.CNT + 7
   READ REC FROM CONTROL, CONO:"MANUAL.INVOICE.PURGE" ELSE REC = NSET
   PRINT SP2:"Manual Invoice Purge - ":REC
   PRINT
   PRINT
   LINE.CNT = LINE.CNT + 3
*--- APS
   PRINT "      ACCOUNT PAYABLE"
   PRINT DASHES[1,27]
   PRINT
   PRINT SP2:"***** Voucher Number *****"
   PRINT
   LINE.CNT = LINE.CNT + 5
   READ REC FROM CONTROL, CONO:"VOUNUMBERS" ELSE REC = ""
   FOR I = 1 TO 12
      IF LINE.CNT >= 57 THEN GOSUB 1000
      IF REC<I> = "" THEN REC<I> = NSET
      PRINT SP2:"Month ":I "R#2":" - ":REC<I>
      LINE.CNT = LINE.CNT + 1
   NEXT I
   REC = ""
   PRINT
   PRINT SP2:"***** Others *****"
   PRINT
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"AUTOVOUCHER" ELSE REC = NSET
   PRINT SP2:"Auto Voucher   - ":REC
   READ REC FROM CONTROL, CONO:"REPAIR-NO" ELSE REC = NSET
   PRINT SP2:"Repair Number  - ":REC
   READ REC FROM CONTROL, CONO:"VEND" ELSE REC = NSET
   PRINT SP2:"Vendor Number  - ":REC
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   PRINT
   PRINT
   LINE.CNT = LINE.CNT + 2
   IF LINE.CNT >= 57 THEN GOSUB 1000
*--- PRS
   PRINT "      PAYROLL"
   PRINT DASHES[1,19]
   PRINT
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"CREDITUNION" ELSE REC = NSET
   PRINT SP2:"Credit Union Code - ":REC
   READ REC FROM CONTROL, CONO:"PETTY.CASH.CODE" ELSE REC = NSET
   PRINT SP2:"Petty Cash Code   - ":REC
   LINE.CNT = LINE.CNT + 2
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"401K" ELSE REC = ""
   IF REC<1> = "" THEN REC<1> = NSET
   IF REC<2> = "" THEN REC<2> = NSET
   IF REC<3> = "" THEN REC<3> = NSET
   IF REC<4> = "" THEN REC<4> = NSET
   PRINT SP2:"401K B / M Code   - ":REC<1>
   PRINT SP2:"401K B / U Code   - ":REC<2>
   PRINT SP2:"401K A / M Code   - ":REC<3>
   PRINT SP2:"401K A / U Code   - ":REC<4>
   PRINT
   PRINT
   IF LINE.CNT + 6 >= 57 THEN GOSUB 1000
   LINE.CNT = LINE.CNT + 6
*--- GLS
   PRINT "      GENERAL LEDGER"
   PRINT DASHES[1,27]
   PRINT
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"ACCT.PERIODS" ELSE REC = NSET
   PRINT SP2:"Number of Periods : ":REC<1>
   PRINT
   LINE.CNT = LINE.CNT + 2
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"FISCAL" ELSE REC = NSET
   PRINT SP2:"Fiscal    - ":REC<1,DCOUNT(REC<1>,@VM)>
   READ REC FROM CONTROL, CONO:"APVFISCAL" ELSE REC = NSET
   PRINT SP2:"Apvfiscal - ":REC<1,DCOUNT(REC<1>,@VM)>
   READ REC FROM CONTROL, CONO:"APDFISCAL" ELSE REC = NSET
   PRINT SP2:"Apdfiscal - ":REC<1,DCOUNT(REC<1>,@VM)>
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"ARSFISCAL" ELSE REC = NSET
   PRINT SP2:"Arsfiscal - ":REC<1,DCOUNT(REC<1>,@VM)>
   READ REC FROM CONTROL, CONO:"ARCFISCAL" ELSE REC = NSET
   PRINT SP2:"Arcfiscal - ":REC<1,DCOUNT(REC<1>,@VM)>
   READ REC FROM CONTROL, CONO:"PRFISCAL" ELSE REC = NSET
   PRINT SP2:"Prfiscal  - ":REC<1,DCOUNT(REC<1>,@VM)>
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"JCFISCAL" ELSE REC = NSET
   PRINT SP2:"Jcfiscal  - ":REC<1,DCOUNT(REC<1>,@VM)>
   READ REC FROM CONTROL, CONO:"ICFISCAL" ELSE REC = NSET
   PRINT SP2:"Icfiscal  - ":REC<1,DCOUNT(REC<1>,@VM)>
   READ REC FROM CONTROL, CONO:"FAFISCAL" ELSE REC = NSET
   PRINT SP2:"Fafiscal  - ":REC<1,DCOUNT(REC<1>,@VM)>
   READ REC FROM CONTROL, CONO:"OPFISCAL" ELSE REC = NSET
   PRINT SP2:"Opfiscal  - ":REC<1,DCOUNT(REC<1>,@VM)>
   PRINT
   LINE.CNT = LINE.CNT + 5
   IF LINE.CNT >= 57 THEN GOSUB 1000
*--- OPS
   PRINT "      ORDER PROCESSING"
   PRINT DASHES[1,27]
   PRINT
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"ORDER" ELSE REC = NSET
   PRINT SP2:"Order            - ":REC
*T24541v
*  READ REC FROM CONTROL, CONO:"DIE" ELSE REC = NSET
*  PRINT SP2:"Die              - ":REC
*T24541^
   READ REC FROM CONTROL, CONO:"BOM" ELSE REC = NSET
   PRINT SP2:"Bill Of Material - ":REC
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
*T26556 v
*  READ REC FROM CONTROL, CONO:"DAILY.PALLET" ELSE REC = NSET
*  PRINT SP2:"Pallet           - ":REC
*T26556 ^
   READ REC FROM CONTROL, CONO:"DAILY.BOL" ELSE REC = NSET
   PRINT SP2:"Bill Of Lading   - ":REC
   READ REC FROM CONTROL, CONO:"NEXT.INVOICE.NO" ELSE REC = NSET
   PRINT SP2:"Order Invoice    - ":REC
   READ REC FROM CONTROL, CONO:"ORDER.INVOICE.PURGE" ELSE REC = NSET
   PRINT SP2:"Invoice Purge    - ":REC
   PRINT
   PRINT
   LINE.CNT = LINE.CNT + 5
   IF LINE.CNT >= 57 THEN GOSUB 1000
*--- ICS
   PRINT "      INVENTORY"
   PRINT DASHES[1,21]
   PRINT
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"TRANSFER" ELSE REC = NSET
   PRINT SP2:"Transfer           - ":REC
   READ REC FROM CONTROL, CONO:"INV.COST.ADJ" ELSE REC = NSET
   PRINT SP2:"Cost Adjustment    - ":REC
   READ REC FROM CONTROL, CONO:"INV.ADJ.HIST" ELSE REC = NSET
   PRINT SP2:"History Adjustment - ":REC
   READ REC FROM CONTROL, CONO:"INV.REC.ADJ" ELSE REC = NSET
   PRINT SP2:"Receipt Adjustment - ":REC
   READV VAR1 FROM CONTROL,CONO:'BARCODE.INV.SEQ',1 ELSE VAR1 = 'NEED SETUP'
   PRINT SP2:'Barcode Sequence   - ':VAR1
   LINE.CNT = LINE.CNT + 5
   IF LINE.CNT >= 57 THEN GOSUB 1000
   PRINT
   PRINT
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
*--- POS
   PRINT "      PURCHASE ORDER"
   PRINT DASHES[1,26]
   PRINT
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"POCODE" ELSE REC = NSET
   PRINT SP2:"Po Number         - ":REC
   READ REC FROM CONTROL, CONO:"OPOCODE" ELSE REC = NSET
   PRINT SP2:"Outside Po Number - ":REC
   READ REC FROM CONTROL, CONO:"MPOCODE" ELSE REC = NSET
   PRINT SP2:"Misc. Po Number   - ":REC
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   PRINT
   PRINT
   LINE.CNT = LINE.CNT + 2
   IF LINE.CNT >= 57 THEN GOSUB 1000
*--- JES
   PRINT "      ESTIMATING"
   PRINT DASHES[1,22]
   PRINT
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"EST.SEQ.NUM" ELSE REC = NSET
   PRINT SP2:"Estimate Number - ":REC
   PRINT
   PRINT
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
*--- JCS
   PRINT "      JOB COSTING"
   PRINT DASHES[1,23]
   PRINT
   PRINT SP2:"***** Daily Files *****"
   PRINT
   LINE.CNT = LINE.CNT + 5
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"DAILY.TIME" ELSE REC = NSET
   PRINT SP2:"Time             - ":REC
   READ REC FROM CONTROL, CONO:"DAILY.MATL" ELSE REC = NSET
   PRINT SP2:"Material         - ":REC
   READ REC FROM CONTROL, CONO:"DAILY.SHIP" ELSE REC = NSET
   PRINT SP2:"Shipping         - ":REC
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"DAILY.OSP" ELSE REC = NSET
   PRINT SP2:"Outside Purchase - ":REC
   READ REC FROM CONTROL, CONO:"DAILY.MISC" ELSE REC = NSET
   PRINT SP2:"Miscellaneous    - ":REC
   READ REC FROM CONTROL, CONO:"DAILY.REVERSE" ELSE REC = NSET
   PRINT SP2:"Reverse          - ":REC
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"DAILY.RCLS" ELSE REC = NSET
   PRINT SP2:"Reclassification - ":REC
   PRINT
   PRINT SP2:"***** Processed to Job *****"
   PRINT
   LINE.CNT = LINE.CNT + 4
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"JOBNO" ELSE REC = NSET
   PRINT SP2:"Starting Job Num - ":REC
   READ REC FROM CONTROL, CONO:"JOB.REVERSE" ELSE REC = NSET
   PRINT SP2:"Reverse          - ":REC
   READ REC FROM CONTROL, CONO:"JOB.RCLS" ELSE REC = NSET
   PRINT SP2:"Reclassification - ":REC
   READ REC FROM CONTROL, CONO:"NEXT.INVOICE.NO" ELSE REC = NSET
   PRINT SP2:"Invoice          - ":REC
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   PRINT
   PRINT SP2:"***** Others *****"
   PRINT
   LINE.CNT = LINE.CNT + 3
   IF LINE.CNT >= 57 THEN GOSUB 1000
   READ REC FROM CONTROL, CONO:"CUSTOMER" ELSE REC = NSET
   PRINT SP2:"Customer      - ":REC
   READ REC FROM CONTROL, CONO:"INVOICE.PURGE" ELSE REC = NSET
   PRINT SP2:"Invoice Purge - ":REC
*--- PSS
*  PRINT "      PRODUCTION SCHEDULING"
*  PRINT DASHES[1,33]
*  PRINT
*  READ REC FROM CONTROL, CONO:"PSS.OT.SCHED" ELSE REC = NSET
*  PRINT SP2:"Overtime Allowed - ":OCONV(REC,"MD2")
   GOTO 99999
1000 *
   PRINT CHAR(12)
   PAGE.NO = PAGE.NO + 1
*T26556 v
*  HEAD1 = 'DATE - ' : OCONV(DATE(), "D2/") "L#8"
*  HEAD1 = HEAD1 : SPACE(26) : "C O N T R O L   S E T U P   L I S T I N G"
*  HEAD1 = HEAD1 : SPACE(32) : "PAGE : ":PAGE.NO
*  HEAD2 = SPACE(53) : "FOR COMPANY - ":CONO "L#3"
*  HEAD3 = SPACE(53) : MENUS.CONTROL 
*  HEAD4 = SPACE(40) : DASHES[1,42] 
*  PRINT HEAD1
*  PRINT HEAD2
*  PRINT HEAD3
*  PRINT HEAD4
   HEAD3 = "FOR COMPANY : ":CONO
   HEAD4 = "FOR ACCOUNT : ":REC<3>
*T28418   HEAD1 = HEAD1 : PAGE.NO
  HLINE1 = HEAD1:PAGE.NO; *T28418
*T28418   PRINT HEAD1
   PRINT HLINE1; *T28418
   PRINT HEAD2
   PRINT HEAD3
   PRINT HEAD4
*T26556 ^
   PRINT
   LINE.CNT = 6
   RETURN
* END OF SUBROUTINE
99999 *
   PRINTER CLOSE
   PRINTER OFF
   RETURN
END
