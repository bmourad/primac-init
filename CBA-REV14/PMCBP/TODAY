     IN.PROC = 1
     PROCREAD BUFFER ELSE IN.PROC = 0
     IF IN.PROC THEN
        I = 1
        LOOP WHILE BUFFER<I> # '' DO
           I = I + 1
        REPEAT
        BUFFER<I> = DATE()
        PROCWRITE BUFFER
     END
     END
