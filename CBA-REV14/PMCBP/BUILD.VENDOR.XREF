*COPY>PMC.CPYLIB>VEND
OPEN '','VEND' TO VEND ELSE STOP
OPEN '','VENDOR.XREF' TO VEND.XREF ELSE STOP
OPEN '','PREFIX' TO PREFIX ELSE STOP
OLD.NAME = ''
CLEARFILE VEND.XREF
SELECT VEND
DATA = 1
LOOP
READNEXT ID ELSE DATA = 0
WHILE DATA DO
CONO = ID[1,3]
VEND.NUM = ID[4,99]
MATREAD VEND.REC FROM VEND, ID ELSE GOTO 99
CALL GEN.XREF.MAINT(CONO,VEND.NUM,OLD.NAME,VEND.DESC,VEND.XREF,PREFIX)
99 REPEAT
END
