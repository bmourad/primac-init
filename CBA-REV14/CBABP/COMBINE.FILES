OPEN '','HOLD.FILE' TO HOLD.FILE ELSE
ERRMSG = 'HOLD.FILE IS MISSING'
GOSUB 91000
GOTO 99999
END
OPEN '','HOLD.FILE.2' TO HOLD.FILE.2 ELSE
ERRMSG = 'HOLD.FILE.2 IS MISSING'
GOSUB 91000
GOTO 99999
END
MORE = 1
REC = ''
REC2 = ''
LOOP
READNEXT ID ELSE MORE = 0
WHILE MORE DO
READU REC2 FROM HOLD.FILE.2, ID THEN
READU REC FROM HOLD.FILE, ID ELSE
WRITE REC2 ON HOLD.FILE, ID
END
END ELSE
MORE = 0
END
REPEAT
GOTO 99999
91000*
PRINT @(0,23):ERRMSG:
INPUT REPLY
RETURN
99999*
END
