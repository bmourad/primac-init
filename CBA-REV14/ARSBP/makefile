
#
#  Make file for BP's in ARSBP
#

BAS = BASIC 
BAS.OPT =
CAT = CATALOG
CAT.OPT = FORCE DIRECT
LOCAL.CAT.OPT = FORCE LOCAL 
BP = ARSBP
MK = ../SAVEDLISTS/ARSBP

ALL: _AGING.DATES _ARC.EOM.CLOSE _ARC.EOM.LIST _ARC.EOM.POST _ARS.EOM.CLOSE _ARS.EOM.LIST _ARS.EOM.POST _ARS.OVERDUE.CUST _ARS.OVERDUE.SLSM _ARS.OVERDUE.SLSM.1 _CASH.REC.INV _CASH.REC.MAINT _CASH.REC.REVERSE _CASH.REC.UPDATE _CLR.MANUAL.INV _CUST.SUM.AGING _INV.DATE.MAINT _MANUAL.INV.EDIT.LIST _MANUAL.INV.EDIT.LIST.F4 _MANUAL.INVOICE.ALIGN _MANUAL.INVOICE.MAINT _MANUAL.INVOICE.MAINT.F4 _MANUAL.INVOICE.PRINT _MANUAL.INVOICE.PRINT.F4 _MINVOICE.REG.REF _MISC.CASH.REC.MAINT _OPEN.RECV.AGING _OPEN.RECV.AGING.SLSMN _OPEN.RECV.MAINT _POST.MANUAL.INVOICE _ROLODEX.PRT _SALES.JOURNAL.DRIVER _SALES.JOURNAL.RPT _STATEMENTS 


