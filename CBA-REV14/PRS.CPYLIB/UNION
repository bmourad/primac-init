**********************************
*   UNION CODE FILE              *
*                                *
*   KEY = CONO:CODE              *
**********************************
*
    UNN.REC.SIZE = 10
    DIM UNN.REC(10)
*
*
    EQU UNN.DUES TO UNN.REC(1)  ;* UNION DUES
    EQU UNN.WEL  TO UNN.REC(2)  ;* UNION WELFARE
    EQU UNN.PEN  TO UNN.REC(3)  ;* UNION PENSION
    EQU UNN.DESC TO UNN.REC(4)  ;* UNION CODE DESCRIPTION
*
