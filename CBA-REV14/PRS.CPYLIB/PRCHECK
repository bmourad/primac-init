***********************
* REVISION  - [08.0]  *
*  KEY = CONO : GL ACCOUNT : CHECK NUMBER
****************************
PCH.REC.SIZE = 10
DIM PCH.REC(10)
EQU PCH.EMP       TO PCH.REC(1)
EQU PCH.CHK.DATE  TO PCH.REC(2)
EQU PCH.CHK.AMT   TO PCH.REC(3)
EQU PCH.AMT.PD    TO PCH.REC(4)
EQU PCH.CLEARED   TO PCH.REC(5); * 1 = CLEARED
EQU PCH.REMARKS   TO PCH.REC(6)
EQU PCH.CODE      TO PCH.REC(7); * P = PREPAID , C = CLEARED , V = VOID
