JSTAT.REC.SIZE = 25
DIM JSTAT.REC(25)
EQU JSTAT.OPO.NO TO JSTAT.REC(1)
EQU JSTAT.OPO.DATE TO JSTAT.REC(2)
EQU JSTAT.OPO.AMT TO JSTAT.REC(3)
EQU JSTAT.OPO.QTY TO JSTAT.REC(4)
EQU JSTAT.SHP.QTY TO JSTAT.REC(5)
EQU JSTAT.SHP.PROD TO JSTAT.REC(6)
EQU JSTAT.SHP.WHSE TO JSTAT.REC(7)
EQU JSTAT.SHP.LOC TO JSTAT.REC(8)
EQU JSTAT.SHP.R.S.ID TO JSTAT.REC(9)
EQU JSTAT.SHP.TRANS TO JSTAT.REC(10)
EQU JSTAT.SHP.MLTP TO JSTAT.REC(11)
EQU JSTAT.SHP.PCS TO JSTAT.REC(12)
EQU JSTAT.SHP.LOC.DESC TO JSTAT.REC(13)
EQU JSTAT.FNG.PROD TO JSTAT.REC(14)
EQU JSTAT.FNG.WHSE TO JSTAT.REC(15)
EQU JSTAT.FNG.QTY TO JSTAT.REC(16)
EQU JSTAT.FNG.TRANS TO JSTAT.REC(17)
EQU JSTAT.PROD.LINE TO JSTAT.REC(18)
