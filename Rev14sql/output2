SELECT DISTINCT JLB_JOB_TIME.CONO , ESTJA_LB_DEPT_MV.ESTJA_LB_ACT_HRS , ESTJA_LB_DEPT_MV.ESTJA_LB_HRS , ESTJA_LB_DEPT_MV.ESTJA_LB_DEPT , ESTJA_LB_DEPT_MV.ESTJA_LB_CCTR , ESTJA_LB_DEPT_MV.ESTJA_LB_OPER FROM JLB_JOB_TIME , ESTJA_LB_DEPT_MV WHERE ( JLB_JOB_TIME.JLB_CONOJOB = ESTJA_LB_DEPT_MV.ESTJA_ID ) and ( JLB_JOB_TIME.JLB_DEPT = ESTJA_LB_DEPT_MV.ESTJA_LB_DEPT ) and ( JLB_JOB_TIME.JLB_CCTR = ESTJA_LB_DEPT_MV.ESTJA_LB_CCTR ) and ( JLB_JOB_TIME.JLB_OPER = ESTJA_LB_DEPT_MV.ESTJA_LB_OPER ) and ( ( JLB_JOB_TIME.CONO = '001' ) AND ( JLB_JOB_TIME.JLB_DATE >= '1997-04-10' ) AND ( JLB_JOB_TIME.JLB_DATE_ENDING <= '1997-04-10' ) AND ( JLB_JOB_TIME.JLB_EST_NUM <> '' ) ) ORDER BY JLB_JOB_TIME.CONO ASC ;
