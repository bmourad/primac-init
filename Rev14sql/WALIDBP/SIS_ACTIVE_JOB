   SUBROUTINE SIS_ACTIVE_JOB (ACTIVE, JOB_STATUS, JOB_TRACK_DATE)
*
   ACTIVE = 0
   CALL JOB.STATUS.SUB (JOB_STATUS, JOB_TRACK_DATE, STATUS)
   BEGIN CASE
      CASE STATUS = "BOOKED"
         ACTIVE = 1
      CASE STATUS = "IN PROCESS"
         ACTIVE = 1
      CASE STATUS = "REOPENED"
         ACTIVE = 1
   END CASE
   RETURN
END
