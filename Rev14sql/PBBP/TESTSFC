*COPY>CPYLIB>CHAR
PROMPT''
CRT @(0,5):CL:
CRT @(0,5):"INPUT INTERNAL DATE ":
INPUT SFC
NEW.DATE = OCONV(SFC,'D/')
CRT @(0,6):CL:
CRT @(0,7):CL:
CRT @(0,7):NEW.DATE:
CRT @(0,8):CL:
CRT @(0,9):CL:
NEW.DATE = NEW.DATE[1,6] : NEW.DATE[7,4] + 1
CRT @(0,9):NEW.DATE:
CRT @(0,10):CL:
CRT @(0,11):CL:
CRT @(0,11):"NEW INTERNAL DATE ":ICONV(NEW.DATE,'D/')
INPUT XXX
END
